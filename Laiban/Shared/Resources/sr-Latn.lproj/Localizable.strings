"trashmonster_aqua-liv_description" = "Aqua-Liv postoji od osvita vremena, a čak je upoznao i dinosauruse i Vikinge! Aqua-Liv ne sakuplja рubre, ali kontroliљe svu vodu na svetu.";
"trashmonster_batman_description" = "Betmen je optuћen. Kada mu ponete sa energijom, kreće na put otkrivanja i traži nove baterije.";
"trashmonster_elli_description" = "Elektriиne stvari su Elisovi favoriti. Fenovi za kosu, tableti i mobilni telefoni - ona se brine o svima njima";
"trashmonster_farao_description" = "Farao je malo opasan. Ljuti se kao pčela kada ne nađe svoj opasni otpad. Ono što najviše voli je stari sušeni lak za nokte i sprej za kosu.";
"trashmonster_flamman_description" = "Vatra i vrućina su dve stvari oko kojih je Flamman strastven. Ona sakuplja sve što niko drugi ne želi. Pelene, četke za sudove i cucle, to su njene stvari!";
"trashmonster_glas-klara_description" = "Glas-Klara svetluca i svetluca kao fina staklena kugla. Svojim velikim prelepim okom skenira okolinu i traži neodlikovano staklo, koje su najlepše stvari koje zna.";
"trashmonster_glasmo_description" = "Glasmo voli tamne staklene predmete ali se plaši da će biti oštećen slomljenim staklom. Sa svojih šest uskih nogu, prošvercuje se kroz kante za smeće i kontejnere kako bi pronašao najfinije flaše i tegle.";
"trashmonster_kompostina_description" = "Kora od banane, jezgra jabuke ili testenine, Kompostina sakuplja većinu stvari u načinima ishrane. Ona zna da je važno da smanjimo bacanje hrane, pa je dodatno srećna kada pronađe ostatke hrane koji zapravo nisu jestivi.";
"trashmonster_lysien_description" = "Lisien se stvarno plaši mraka, misli da u mraku ima opasnih i strašnih čudovišta. Na vrhu glave ima svoju LED lampu i u ruci uvek sa sobom nosi fluorescentnu lampu. On ih pali čim tama počne da pada.";
"trashmonster_metallika_description" = "Metal je najbolji koji Metalika zna. On skuplja sve što je teško dodirnuti i pravi zvukove kada ga udari, baš kao i on samog!";
"trashmonster_nova-kras_description" = "Polomljene čaše za piće, krofne ili glineni lonci, to su stvari koje Nova-Kras voli.";
"trashmonster_pantomen_description" = "Pantomen sakuplja sve konzerve i flaše koje na sebi imaju oznaku depozita. Pantomen takođe ima trag na njoj, samo da se ne bi spetljao sa Plastisom ili Metaliko.";
"trashmonster_pappis_description" = "Ima dosta papira, što je dobro za Pappisa koji voli da skuplja staru papirnu ambalažu. Kelog paketići, kutije za cipele i krem paketići su neke od njegovih omiljenih stvari.";
"trashmonster_plastis_description" = "Plastičnu ambalažu je dobro imati, barem prema Plastisu koji misli da je stvaranje izuma i novih igračaka iz stare ambalaže zaista zabavno!";
"trashmonster_prassel_description" = "Prasel je mogao da čita novine celu večnost. Nerešene ukrštenice, smešne priče i jučerašnje vesti - SVE ŠTO želi da pročita.";
"trashmonster_press_to_learn_more" = "Dodirnite čudovište da biste saznali više o tome.";
"trashmonster_stubben_description" = "Patrljak sakuplja lišće, grančice i cveće i najbolje od svega je što ono što sakuplja postaje novo zemljište u kojem možete posaditi novo drveće ili cveće.";
"trashmonster_textilia_description" = "Textilia je priliиno nova u bandi. Skuplja staru i iznošenu odeću i obuću, ali i odeću koja je lepa i može ponovo da se koristi.";
"trashmonster_towalett_description" = "Pišaćka, kaka i toalet papir su jedine stvari koje Towalett sakuplja. Sa svojih pet očiju traži tigove poput pamučnih brisa, duvana ili vlažnih maramica, ovima nije mesto u toaletu!";
"trashmonster_wellmer_description" = "Mnogi krhki paketi dolaze u korumpiranom kartonu, zbog čega se Velmer oseća kao vitez koji štiti krhke stvari. Obožava da koristi korumpirani karton i često pravi zamkove vitezova od starih kutija.";
"undp_goal" = "Gol %@";
"undp_goal_10_description" = "To znači da gde god da živite u svetu, treba da imate slična prava i mogućnosti kao i svi ostali.";
"undp_goal_10_title" = "Cilj 10: Smanjene nejednakosti";
"undp_goal_11_description" = "To znači da svi treba da budu u mogućnosti da žive u sigurnim i dobrim domovima.";
"undp_goal_11_title" = "Cilj 11: Održivi gradovi i kumuniteti";
"undp_goal_12_description" = "To znači da treba da kupujemo samo stvari koje su nam potrebne bez bacanja i bacanja previše stvari.";
"undp_goal_12_title" = "Cilj 12: Odgovorna potrošnja i proizvodnja";
"undp_goal_13_description" = "Da bismo dobro živeli na našoj planeti, moramo osigurati da klimatske promene budu zaustavljene.";
"undp_goal_13_title" = "Cilj 13: Klimatska akcija";
"undp_goal_14_description" = "To znači da treba da se brinemo o našim okeanima bez smeća i hvatanja previše ribe.";
"undp_goal_14_title" = "Cilj 14: Život ispod vode";
"undp_goal_15_description" = "To znači da treba da vodimo računa o našoj prirodi i svima koji tamo žive, kako o životinjama, tako i o ljudima, kao i o biljkama i insetima.";
"undp_goal_15_title" = "Cilj 15: Život na kopnu";
"undp_goal_16_description" = "To znači da treba da imamo mirna i samo društva.";
"undp_goal_16_title" = "Cilj 16: Mir, pravda i jake institucije";
"undp_goal_17_description" = "Da bismo stvorili bolji svet treba da radimo zajedno, jedni sa drugima, i sa drugim zemljama.";
"undp_goal_17_title" = "Cilj 17: Partnerstva za ciljeve";
"undp_goal_1_description" = "Siromaštvo može da znači da vam nedostaje hrana ili novac, da se ne osećate sigurno ili vam nije dozvoljeno da idete u školu.";
"undp_goal_1_title" = "Cilj 1: Bez siromaštva";
"undp_goal_2_description" = "To znači da svako treba da bude u stanju da jede hranu uzgajanu na dobar način, i da niko ne treba da gladuje.";
"undp_goal_2_title" = "Cilj 2: Nulta glad";
"undp_goal_3_description" = "To znači da svako treba da bude u stanju da živi zdravim i dobrim životom.";
"undp_goal_3_title" = "Cilj 3: Dobro zdravlje i dobro biće";
"undp_goal_4_description" = "To znači da svi treba da budu u mogućnosti da idu u školu i da imaju priliku da uče nove stvari tokom našeg života.";
"undp_goal_4_title" = "Cilj 4: Kvalitetno obrazovanje";
"undp_goal_5_description" = "To znači da su svi jednako vredni, i devojčice i dečaci.";
"undp_goal_5_title" = "Cilj 5: Rodna ravnopravnost";
"undp_goal_6_description" = "To znači da bi svi trebalo da mogu da piju čistu vodu i da mogu da idu u toalet.";
"undp_goal_6_title" = "Cilj 6: Čista voda i sanitarije";
"undp_goal_7_description" = "To znači da svi treba da budu u stanju da imaju struju i toplotu koja je dobra za našu planetu.";
"undp_goal_7_title" = "Cilj 7: Pristupačna i čista energija";
"undp_goal_8_description" = "To znači da svi treba da budu u stanju da se osećaju dobro na poslu. Takođe ne bi trebalo da radite ceo dan, ali i da budete u prilici da se opustite u slobodno vreme.";
"undp_goal_8_title" = "Cilj 8: Pristojan rad i ekonomski rast";
"undp_goal_9_description" = "To znači da proizvodnja naših izuma mora biti zdrava i dobra za nas i našu prirodu.";
"undp_goal_9_title" = "Cilj 9: Industrija, inovacije i infrastruktura";
