"trashmonster_aqua-liv_description" = "Aqua-Liv ir pastāvējis kopš laika rītausmas un ir pat ticies ar dinozauriem un vikingiem! Aqua-Liv nevāc nekādus atkritumus, bet kontrolē visu ūdeni pasaulē.";
"trashmonster_batman_description" = "Betmens ir uzlādēts puisis. Kad viņa enerģija sāk izsīkt, viņš dodas atklāšanas ceļojumā un meklē jaunas baterijas.";
"trashmonster_elli_description" = "Elektriskās lietas ir Elisa mīļākās lietas. Matu žāvētāji, tabletes un mobilie tālruņi - viņa par tiem visiem rūpējas";
"trashmonster_farao_description" = "Farao ir nedaudz bīstams. Viņa dusmojas kā bite, kad neatrod savus bīstamos atkritumus. Tas, ko viņa mīl visvairāk, ir veca žāvēta nagu laka un matu laka.";
"trashmonster_flamman_description" = "Uguns un karstums ir divas lietas, par kurām Flammans aizraujas. Viņa savāc visu, ko neviens cits nevēlas. Autiņbiksītes, trauku sukas un knupīši, tās ir viņas lietas!";
"trashmonster_glas-klara_description" = "Glas-Klara mirdz un mirdz kā smalkā stikla bumba, kas viņa ir. Ar savu lielo skaisto aci viņa skenē apkārtni un meklē nekrāsotu stiklu, kas ir vislabākās lietas, ko viņa zina.";
"trashmonster_glasmo_description" = "Glasmo mīl tumšus stikla priekšmetus, bet baidās, ka tos sabojās salauzts stikls. Ar savām sešām šaurajām kājām viņš līst pa atkritumu tvertnēm un konteineriem, lai atrastu smalkākās pudeles un burkas.";
"trashmonster_kompostina_description" = "Banānu mizas, ābolu serdes vai makaroni, Kompostina savāc lielāko daļu lietu pārtikas veidos. Viņa zina, ka ir svarīgi, lai mēs samazinātu pārtikas atkritumus, tāpēc viņa ir īpaši laimīga, kad atrod pārpalikumus, kas patiesībā nav ēdami.";
"trashmonster_lysien_description" = "Lysien patiešām baidās no tumsas, viņš domā, ka tumsā ir bīstami un biedējoši monstri. Virs galvas viņam ir sava LED lampa, un rokā viņš vienmēr nēsā līdzi dienasgaismas lampu. Viņš tos apgaismo, tiklīdz tumsa sāk krist.";
"trashmonster_metallika_description" = "Metāls ir labākais, ko Metallika zina. Viņš kolekcionē visu, kam ir grūti pieskarties, un, to trāpot, izdara skaņas, tāpat kā viņš pats!";
"trashmonster_nova-kras_description" = "Salauztas dzeršanas brilles, trauki vai māla podi, tās ir lietas, kas patīk Nova-Kras.";
"trashmonster_pantomen_description" = "Pantomens savāc visas kārbas un pudeles, uz kurām ir depozīta zīme. Pantomenam ir arī zīme uz viņas, tikai tāpēc, lai viņa netiktu sajaukta ar Plastisu vai Metaliku.";
"trashmonster_pappis_description" = "Ir daudz papīra, kas ir labs Pappisam, kuram patīk savākt veco papīra iepakojumu. Kellog iepakojumi, apavu kastes un krējuma iepakojumi ir dažas no viņa iecienītākajām lietām.";
"trashmonster_plastis_description" = "Plastmasas iepakojums ir labs, vismaz pēc Plastis domām, kurš domā, ka izgudrojumu un jaunu rotaļlietu radīšana no vecā iepakojuma ir patiešām jautra!";
"trashmonster_prassel_description" = "Prassel varēja lasīt avīzes uz mūžību. Neatrisinātas krustvārdu mīklas, smieklīgi stāsti un vakardienas ziņas - VISS, ko viņa vēlas lasīt.";
"trashmonster_press_to_learn_more" = "Pieskarieties briesmonim, lai uzzinātu vairāk par to.";
"trashmonster_stubben_description" = "Celms savāc lapas, zarus un ziedus, un labākais no visiem ir tas, ka tas, ko viņš savāc, kļūst par jaunu augsni, kurā jūs varat stādīt jaunus kokus vai ziedus.";
"trashmonster_textilia_description" = "Textilia ir diezgan jauns bandai. Viņa kolekcionē vecas un novalkātas drēbes un apavus, kā arī apģērbus, kas ir jauki un kurus var izmantot vēlreiz.";
"trashmonster_towalett_description" = "Pee, poop un tualetes papīrs ir vienīgās lietas, ko Towalett savāc. Ar savām piecām acīm viņa meklē zagļus, piemēram, vates tamponus, tabaku vai mitrās salvetes, tie neietilpst tualetē!";
"trashmonster_wellmer_description" = "Daudzi trausli iepakojumi ir gofrētā kartonā, tāpēc Wellmer jūtas kā bruņinieks, kurš aizsargā trauslas lietas. Viņam patīk atkārtoti izmantot gofrēto kartonu un bieži izgatavo bruņinieku pilis no vecajām kastēm.";
"undp_goal" = "Mērķa %@";
"undp_goal_10_description" = "Tas nozīmē, ka neatkarīgi no tā, kur jūs dzīvojat pasaulē, jums vajadzētu būt līdzīgām tiesībām un iespējām kā visiem pārējiem.";
"undp_goal_10_title" = "10. mērķis: nevienlīdzības mazināšana";
"undp_goal_11_description" = "Tas nozīmē, ka ikvienam ir jāspēj dzīvot drošās un labās mājās.";
"undp_goal_11_title" = "11. mērķis: Ilgtspējīgas pilsētas un imunitāte";
"undp_goal_12_description" = "Tas nozīmē, ka mums vajadzētu pirkt tikai to, kas mums nepieciešams, netērējot vai neizmetot pārāk daudz lietu.";
"undp_goal_12_title" = "12. mērķis: atbildīgs patēriņš un ražošana";
"undp_goal_13_description" = "Lai labi dzīvotu uz mūsu planētas, mums ir jānodrošina, ka klimata pārmaiņas tiek apturētas.";
"undp_goal_13_title" = "13. mērķis: rīcība klimata politikas jomā";
"undp_goal_14_description" = "Tas nozīmē, ka mums ir jārūpējas par saviem okeāniem, nepiedrazojot un nenoķerot pārāk daudz zivju.";
"undp_goal_14_title" = "14. mērķis: Dzīve zem ūdens";
"undp_goal_15_description" = "Tas nozīmē, ka mums ir jārūpējas par savu dabu un visiem, kas tur dzīvo, gan par dzīvniekiem, gan cilvēkiem, kā arī par augiem un ieliktņiem.";
"undp_goal_15_title" = "15. mērķis: dzīve uz sauszemes";
"undp_goal_16_description" = "Tas nozīmē, ka mums ir vajadzīga miermīlīga un taisnīga sabiedrība.";
"undp_goal_16_title" = "16. mērķis: miers, taisnīgums un spēcīgas iestādes";
"undp_goal_17_description" = "Lai radītu labāku pasauli, mums ir jāstrādā kopā, savā starpā un ar citām valstīm.";
"undp_goal_17_title" = "17. mērķis: partnerības mērķu sasniegšanai";
"undp_goal_1_description" = "Nabadzība var nozīmēt, ka jums trūkst pārtikas vai naudas, jūs nejūtaties droši vai jums nav atļauts iet uz skolu.";
"undp_goal_1_title" = "1. mērķis: nabadzības izskaušana";
"undp_goal_2_description" = "Tas nozīmē, ka ikvienam vajadzētu būt iespējai ēst pārtiku, kas audzēta labā veidā, un ka nevienam nevajadzētu būt izsalkušam.";
"undp_goal_2_title" = "2. mērķis: izskaust badu";
"undp_goal_3_description" = "Tas nozīmē, ka ikvienam ir jāspēj dzīvot veselīgu un labu dzīvi.";
"undp_goal_3_title" = "3. mērķis: laba veselība un labklājība";
"undp_goal_4_description" = "Tas nozīmē, ka ikvienam ir jābūt iespējai doties uz skolu un būt iespējai apgūt jaunas lietas visā mūsu dzīvē.";
"undp_goal_4_title" = "4. mērķis: kvalitatīva izglītība";
"undp_goal_5_description" = "Tas nozīmē, ka visi ir vienlīdz vērtīgi gan meitenes, gan zēni.";
"undp_goal_5_title" = "5. mērķis: dzimumu līdztiesība";
"undp_goal_6_description" = "Tas nozīmē, ka ikvienam vajadzētu būt iespējai dzert tīru ūdeni un spēt doties uz tualeti.";
"undp_goal_6_title" = "6. mērķis: tīrs ūdens un sanitārija";
"undp_goal_7_description" = "Tas nozīmē, ka ikvienam ir jāspēj iegūt elektrību un siltumu, kas ir labs mūsu planētai.";
"undp_goal_7_title" = "7. mērķis: cenas ziņā pieejama un tīra enerģija";
"undp_goal_8_description" = "Tas nozīmē, ka ikvienam ir jāspēj justies labi darbā. Jums arī nevajadzētu strādāt visu dienu, bet arī spēt atpūsties brīvajā laikā.";
"undp_goal_8_title" = "8. mērķis: pienācīgas kvalitātes nodarbinātība un ekonomikas izaugsme";
"undp_goal_9_description" = "Tas nozīmē, ka mūsu izgudrojumu ražošanai jābūt veselīgai un labai mums un mūsu dabai.";
"undp_goal_9_title" = "9. mērķis: rūpniecība, inovācijas un infrastruktūra";
