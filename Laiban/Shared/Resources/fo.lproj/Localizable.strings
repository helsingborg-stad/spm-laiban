"trashmonster_elli_description" = "El elal ting eru yndis yndis. Hair dryktar, tablets og fartelefonir - hon tekur sær av teimum øllum";
"trashmonster_farao_description" = "Farao er eitt sindur vandamikið. Hon verður ill sum ein býflugu, tá hon ikki finnur hennara svøru. Tað, sum hon elskar, er gomul, tá ið hon er í ferð við at gera.";
"trashmonster_flamman_description" = "Eldur og hita eru tvey ting, sum Flamman er fyri. Hon samlar alt, sum eingin annar vil. Diapers, dish bustar og friðarhugar, tað er hennara ting!";
"trashmonster_glas-klara_description" = "Glas-Klara glimmerar og shimmerum sum tann góða bóltin í glasi, sum hon er. Við sínum stóra eyga, meðan hon sýgur seg upp í umstøðurnar og sær út til at vera eitt glas, sum hon veit.";
"trashmonster_glasmo_description" = "Glasmo elskar myrka glas lutir, men er ræðslusligin av at verða skaddur av brotna glasi. Við sínum seks smølum beinum, sníkti hann seg ígjøgnum rusk og containerar at finna ta góðu fløskur og jar.";
"trashmonster_kompostina_description" = "Banana flysur, súreplið core ella pasta, Kompostina savna flestu ting í mati. Hon veit, at tað er týdningarmikið, at vit lækka matin, so hon er eyka glað, tá hon finnur av, at tað ikki er nakað týdningarmikið.";
"trashmonster_lysien_description" = "Lysien er sera bangin fyri myrkri, heldur hann, at tað er vandamikið og ræðandi skrímsl. Á høvdinum hevur hann sína egnu lampu og í hondini, hevur hann altíð borið eina fluorescent lamp við honum. Hann ljósar hesi so skjótt sum myrkrið byrjar at fella.";
"trashmonster_metallika_description" = "Metal er tann besti Metallika veit. Hann samlar alt, sum er ringt at nerta og ljóðar, tá ið hann sló tað, júst sum seg sjálvan!";
"trashmonster_nova-kras_description" = "Broken drekka brillur, crockery ella clay pots, hesi eru tingini, sum Nova-Kras dámar.";
"trashmonster_pantomen_description" = "Pantomen savna allar tær kunnu og fløskur, sum hava sett á tey. Pantomen hevur eisini eitt mark á henni, bara so hon verður ikki blandaður við Plastis ella Metallika.";
"trashmonster_pappis_description" = "Tað er nóg mikið av pappíri, sum er gott fyri pappír, sum dámar at savna gamlar pappír. Kellog pakkar, lissur og róma pakkar eru nøkur av sínum yndis tingum.";
"trashmonster_plastis_description" = "Plastic packaging er gott at hava, í minsta lagi eftir Plastis, sum heldur, at skapa uppfinningar og nýggj leikur frá gomlum packaging er veruliga stuttligt!";
"trashmonster_prassel_description" = "Prassel kundi lisið tíðindabløð fyri ævir. Ótrúligt, at tað er stuttligt, stuttligt søgur og í gjár, - EVERYTHING hon vil lesa.";
"trashmonster_press_to_learn_more" = "Tap eitt ódjór at læra meira um tað.";
"trashmonster_stubben_description" = "Stump collectar fara, kwigs og blómur og blómur, og tað besta av øllum er, at tað, sum hann savnar nýggj trø ella blómur.";
"trashmonster_textilia_description" = "Textilia er rættiliga nýtt til gangið. Hon samlar gomul og uppslitin klæðir og skógvar, men eisini eru fitt og kunnu eisini nýtast aftur.";
"trashmonster_towalett_description" = "Pee, poop og vesipappír eru tey einastu, sum savna seg. Við fimm eygum sær hon út til thigs sum bummullar, ella vát turkaðar, hesi ikki hoyra á vesinum!";
"trashmonster_wellmer_description" = "Nógvir fragile packages koma í corrugated pappi, sum er hví vælmer kennist sum ein ki náttina, sum verja seg. Hann elskar at seta corrugat pappi og ofta ger ki náttina úr gomlum eskjum.";
"undp_goal" = "Goal %@";
"undp_goal_10_description" = "Hetta merkir, at har tú býrt í heiminum, átti tú at havt somu rættindi og møguleikar sum øll onnur.";
"undp_goal_10_title" = "Goal 10: Reduced ine íe fer";
"undp_goal_11_description" = "Hetta merkir, at øll áttu at verið før fyri at búgva í tryggum og góðum heimi.";
"undp_goal_11_title" = "Goal 11: Sustainable býir og cummunities";
"undp_goal_12_description" = "Hetta merkir, at vit bara skuldu keypt ting, okkum tørvar uttan at blaka alt ov nógv.";
"undp_goal_12_title" = "Goal 12: Responsible consumption and production";
"undp_goal_13_description" = "Fyri at liva væl á okkara gongustjørnu, mugu vit tryggja, at veðurlagsbroytingar eru steðgað.";
"undp_goal_13_title" = "Goal 13: C Veður action";
"undp_goal_14_description" = "Hetta merkir, at vit áttu at tikið okkum av okkara havinum uttan at fáa ov nógvan fisk.";
"undp_goal_14_title" = "Goal 14: Lív undir vatni";
"undp_goal_15_description" = "Hetta merkir, at vit skulu taka okkum av okkara lyndi og øll, sum búgva har, bæði djór og menniskju eins væl og plantur og ísetcs.";
"undp_goal_15_title" = "Goal 15: Lív á land";
"undp_goal_16_description" = "Hetta merkir, at vit skuldu havt friðarlig og bara so nógv.";
"undp_goal_16_title" = "Goal 16: Friður, rættvísi og sterkur.";
"undp_goal_17_description" = "Fyri at skapa ein betri heim, áttu vit at arbeitt saman við hvørjum øðrum, og við øðrum londum.";
"undp_goal_17_title" = "Goal 17: Partnerships for málum.";
"undp_goal_1_description" = "Fátækradømi kann merkja, at tær manglar mat ella pening, kenna seg ikki tryggan, ella ikki sleppa í skúla.";
"undp_goal_1_title" = "Goal 1: No fátækradømi";
"undp_goal_2_description" = "Hetta merkir, at øll áttu at kunna etið mat vaks upp á ein góðan hátt, og at eingin skuldi verið svangur.";
"undp_goal_2_title" = "Goal 2: Zero hungri";
"undp_goal_3_description" = "Hetta merkir, at øll áttu at verið før fyri at liva eitt sunt og gott lív.";
"undp_goal_3_title" = "Goal 3: góða heilsu og væl-being";
"undp_goal_4_description" = "Hetta merkir, at øll áttu at kunna farið í skúla og hava møguleikan at læra nýggj ting ígjøgnum okkara lív.";
"undp_goal_4_title" = "Goal 4: Quality útbúgving";
"undp_goal_5_description" = "Hetta merkir, at øll eru líka virðismikið, báðar gentur og dreingir.";
"undp_goal_5_title" = "Goal 5: Gender javnstøðu";
"undp_goal_6_description" = "Hetta merkir, at øll áttu at kunna drukkið vatn og vera før fyri at fara á vesið.";
"undp_goal_6_title" = "Goal 6: Vaska vatn og sanitation";
"undp_goal_7_description" = "Hetta merkir, at øll áttu at kunna havt streym og hita, sum er gott fyri okkara gongustjørnu.";
"undp_goal_7_title" = "Goal 7: Affordable and rein orku";
"undp_goal_8_description" = "Hetta merkir, at øll áttu at verið før fyri at føla seg væl til arbeiðis. Tú/átti/áttu eisini at verið noyddur/noydd at arbeitt allan dagin, men eisini at slappa av.";
"undp_goal_8_title" = "Goal 8: Decent arbeiði og búskaparligan vøkstur";
"undp_goal_9_description" = "Hetta merkir, at framleiðir okkara uppfinning má vera frískt og góð fyri okkum og okkara lyndi.";
"undp_goal_9_title" = "Goal 9: Try, innovation og infrastructure";
