"trashmonster_farao_description" = "Farao bir az təhlükəlidir. O, təhlükəli nəcisini tapmayanda arı kimi hirslənir. Ən çox sevdiyi şey köhnə qurudulmuş dırnaq cilası və saç spreyidir.";
"trashmonster_flamman_description" = "Alov və istilik – Flammanın ehtiraslı olduğu iki şey. O, başqa heç kimin istəmədiyi hər şeyi toplayır. Diaperlər, xörək fırçaları və pazır, bu onun əşyalarıdır!";
"trashmonster_glas-klara_description" = "Qlas-Klara qlammers və şibyələr onun olduğu zərif şüşə topu kimi. O, böyük gözəl gözü ilə ətrafa skan edir və ən gözəl bildiyi ən gözəl şeylər olan üzdə olmayan şüşə axtarır.";
"trashmonster_glasmo_description" = "Glasmo tünd şüşə cisimləri sevir, lakin sınmış şüşənin zədələnməsindən qorxur. Altı dar ayağı ilə zibil qutularından və qablardan gizlincə keçərək ən gözəl butulkaları və küpləri tapır.";
"trashmonster_kompostina_description" = "Banan peel, alma mərciməkləri və ya makaron, Kompostina yemək yolları ilə əksər şeyləri toplayır. O bilir ki, bizim qida tullantılarımızı azaltmağımız vacibdir, buna görə də əslində yeməli olmayan qalanları tapanda əlavə xoşbəxt olur.";
"trashmonster_lysien_description" = "Lysien həqiqətən qaranlıqdan qorxur, o, qaranlıqda təhlükəli və qorxulu canavarlar olduğunu düşünür. Başının üstündə onun öz LED lampası var və əlində həmişə özü ilə flüoresent lampa daşıyır. Qaranlıq düşməyə başlayan kimi bunları işıqlandırır.";
"trashmonster_metallika_description" = "Metal ən yaxşı Metallika bilir. O, toxunmaq çətin olan hər şeyi toplayır və onu vuranda səs çıxarır, özü kimi!";
"trashmonster_nova-kras_description" = "Qırılmış içki stəkanı, kroker və ya gil qazanlar, bunlar Nova-Krasın xoşuna gələn şeylərdir.";
"trashmonster_pantomen_description" = "Pantomen onların üzərində depozit işarəsi olan bütün konservləri və şüşələri toplayır. Pantomenin üzərində də işarə var. Sadəcə, o, Plastis və ya Metallika ilə qarışmayacaq.";
"trashmonster_pappis_description" = "Kifayət qədər kağız var, hansı ki, köhnə kağız paketləri yığmağı sevən Pappis üçün yaxşıdır. Kellog paketləri, ayaqqabı qutuları və krem paketləri onun ən sevdiyi şeylərdəndir.";
"trashmonster_plastis_description" = "Plastik paketlərin olması yaxşıdır. Ən azından köhnə paketlərdən ixtiralar və yeni oyuncaqlar yaratmağın həqiqətən də əyləncəli olduğunu düşünən Plastis-ə görə!";
"trashmonster_prassel_description" = "Prassel qəzetləri əbədiyyətə qədər oxuya bilirdi. Həll edilməmiş crossword bulmacalar, əyləncəli hekayələr və dünənki xəbərlər - Oxumaq istədiyi HƏR ŞEY.";
"trashmonster_press_to_learn_more" = "Bu barədə daha çox öyrənmək üçün bir canavar dokunun.";
"trashmonster_stubben_description" = "Bu kötük yarpaqları, budaqları və gülləri toplayır və ən yaxşısı odur ki, onun topladığı şeylər yeni torpağa çevrilir ki, onda yeni ağaclar və ya güllər əkmək olar.";
"trashmonster_textilia_description" = "Teksilya banda üçün olduqca yenidir. O, köhnə və geyilmiş paltar və ayaqqabı, eyni zamanda qəşəng və yenidən istifadə edilə bilən paltarlar toplayır.";
"trashmonster_towalett_description" = "Pee, poop və tualet kağızı Towalett-in topladığı yeganə şeylərdir. Beş gözü ilə o, pambıq svab, tütün və ya nəm silmə kimi bud axtarır. Bunlar tualetə aid deyil!";
"trashmonster_wellmer_description" = "Bir çox kövrək paketlər korroziyaya məruz qalan kartonda gəlir. Bu səbəbdən də Uellmer özünü kövrək şeyləri qoruyan knight kimi hiss edir. O, korroziyaya məruz olan kartondan yenidən istifadə etməyi sevir və çox vaxt köhnə qutulardan şövkətlərin qəsrlərini düzəldir.";
"undp_goal" = "Məqsəd %@";
"undp_goal_10_description" = "Bu o deməkdir ki, dünyanın harasında yaşayırsansa yaşa, hamı kimi eyni hüquq və imkanlara malik olmalısan.";
"undp_goal_10_title" = "Məqsəd 10: Bərabərsizliyin azalması";
"undp_goal_11_description" = "Bu o deməkdir ki, hər kəs təhlükəsiz və yaxşı evlərdə yaşaya bilməlidir.";
"undp_goal_11_title" = "Məqsəd 11: Davamlı şəhərlər və cummunities";
"undp_goal_12_description" = "Bu o deməkdir ki, biz yalnız ehtiyac duyduğumuz şeyləri israf etmədən və ya çox şeyi atmadan almalıyıq.";
"undp_goal_12_title" = "Məqsəd 12: Məsuliyyətli istehlak və istehsal";
"undp_goal_13_description" = "Planetimizdə yaxşı yaşamaq üçün iqlim dəyişikliyinin dayandırılmasını təmin etməliyik.";
"undp_goal_13_title" = "13-cü məqsəd: İqlim hərəkəti";
"undp_goal_14_description" = "Bu o deməkdir ki, biz okeanlarımızın qayğısına qalmalı və həddindən artıq balıq tutmalıyıq.";
"undp_goal_14_title" = "14-cü məqsəd: Sudan aşağıda həyat";
"undp_goal_15_description" = "Bu o deməkdir ki, biz öz təbiətimizə və orada yaşayan hər kəsə, həm heyvanlara, həm insanlara, həm də bitkilərə və insetkaya diqqət etməliyik.";
"undp_goal_15_title" = "15-ci məqsəd: Quruda həyat";
"undp_goal_16_description" = "Bu o deməkdir ki, biz sülhpərvər və ədalətli cəmiyyətlərə malik olmalıyıq.";
"undp_goal_16_title" = "Məqsəd 16: Sülh, ədalət və güclü qurumlar";
"undp_goal_17_description" = "Daha yaxşı bir dünya yaratmaq üçün bir yerdə, bir-birimizlə və digər ölkələrlə əməkdaşlıq etməliyik.";
"undp_goal_17_title" = "Məqsəd 17: Məqsədlər üçün həmkarlar ittifaqları";
"undp_goal_1_description" = "Yoxsulluq o deməkdir ki, nə yeməyin, nə pulun var, nə özünü təhlükəsiz hiss edirsən, nə də ki, məktəbə getməyə icazə verilmir.";
"undp_goal_1_title" = "1-ci məqsəd: Yoxsulluq yoxdur";
"undp_goal_2_description" = "Bu o deməkdir ki, hər kəs yaxşı şəkildə yetişdirilən qidanı yeyə bilməlidir. Heç kim ac qalmamalıdır.";
"undp_goal_2_title" = "2-ci məqsəd: Sıfır aclıq";
"undp_goal_3_description" = "Bu o deməkdir ki, hər kəs sağlam və yaxşı həyat sürməyi bacarmalıdır.";
"undp_goal_3_title" = "3-cü məqsəd: Sağlamlıq və rifah";
"undp_goal_4_description" = "Bu o deməkdir ki, hər kəs məktəbə getməyi bacarmalı və həyatımız boyu yeni şeylər öyrənmək imkanına malik olmalıdır.";
"undp_goal_4_title" = "Məqsəd 4: Keyfiyyətli təhsil";
"undp_goal_5_description" = "Bu o deməkdir ki, hər kəs eyni dərəcədə dəyərlidir, həm qızlar, həm də oğlanlar.";
"undp_goal_5_title" = "5-ci məqsəd: Gender bərabərliyi";
"undp_goal_6_description" = "Bu o deməkdir ki, hər kəs təmiz su içməyi və tualetə getməyi bacarmalıdır.";
"undp_goal_6_title" = "Məqsəd 6: Təmiz su və sanitariya";
"undp_goal_7_description" = "Bu o deməkdir ki, hər kəs bizim planetimiz üçün yararlı olan elektrik və istiyə malik olmalıdır.";
"undp_goal_7_title" = "Məqsəd 7: Sərfəli və təmiz enerji";
"undp_goal_8_description" = "Bu o deməkdir ki, hər kəs özünü işdə yaxşı hiss etməyi bacarmalıdır. Bütün günü də işləmək lazım deyil, eyni zamanda boş vaxtınızda dincəlməyi bacarmalısınız.";
"undp_goal_8_title" = "Məqsəd 8: Layiqli iş və iqtisadi inkişaf";
"undp_goal_9_description" = "Bu o deməkdir ki, ixtiralarımızın istehsalı sağlam və bizim üçün və təbiətimiz üçün faydalı olmalıdır.";
"undp_goal_9_title" = "Məqsəd 9: Sənaye, innovasiya və infrastruktur";
