"trashmonster_glas-klara_description" = "Glas-Klara svjetluca i svjetluca kao fina staklena kugla. Svojim velikim lijepim okom skenira okolinu i traži neobojeno staklo, što su najbolje stvari koje zna.";
"trashmonster_glasmo_description" = "Glasmo voli tamne staklene predmete, ali se boji da će biti oštećen razbijenim staklom. Sa svojih šest uskih nogu, šulja se kroz kante za smeće i kontejnere kako bi pronašao najbolje boce i staklenke.";
"trashmonster_kompostina_description" = "Kora od banane, jezgre jabuka ili tjestenina, Kompostina prikuplja većinu stvari na načine hrane. Zna da je važno da smanjimo rasipanje hrane, pa je posebno sretna kada pronađe ostatke koji zapravo nisu jestivi.";
"trashmonster_lysien_description" = "Lysien se stvarno boji mraka, misli da u mraku postoje opasna i zastrašujuća čudovišta. Na vrhu glave ima vlastitu LED svjetiljku, au ruci uvijek nosi fluorescentnu svjetiljku sa sobom. Pali ih čim tama počne padati.";
"trashmonster_metallika_description" = "Metal je najbolji metallika zna. On skuplja sve što je teško dotaknuti i proizvodi zvukove kada ga udari, baš kao i on sam!";
"trashmonster_nova-kras_description" = "Razbijene čaše za piće, posuđe ili glinene posude, to su stvari koje Nova-Kras voli.";
"trashmonster_pantomen_description" = "Pantomen skuplja sve limenke i boce koje imaju oznaku naslaga na sebi. Pantomen također ima oznaku na njoj, samo da se ne bi miješala s Plastisom ili Metallikom.";
"trashmonster_pappis_description" = "Ima puno papira, što je dobro za Pappisa koji voli skupljati staru papirnatu ambalažu. Kellog paketi, kutije za cipele i krem paketi su neke od njegovih omiljenih stvari.";
"trashmonster_plastis_description" = "Plastičnu ambalažu dobro je imati, barem prema Plastisu koji smatra da je stvaranje izuma i novih igračaka iz stare ambalaže zaista zabavno!";
"trashmonster_prassel_description" = "Prassel je mogao čitati novine cijelu vječnost. Neriješene križaljke, smiješne priče i jučerašnje vijesti - SVE što želi pročitati.";
"trashmonster_press_to_learn_more" = "Dodirnite čudovište da biste saznali više o tome.";
"trashmonster_stubben_description" = "Panj skuplja lišće, grančice i cvijeće, a najbolje od svega je da ono što skuplja postaje novo tlo u kojem možete posaditi nova stabla ili cvijeće.";
"trashmonster_textilia_description" = "Textilia je prilično nova u bandi. Skuplja staru i istrošenu odjeću i obuću, ali i odjeću koja je lijepa i može se ponovno koristiti.";
"trashmonster_towalett_description" = "Mokraća, kakica i toaletni papir su jedine stvari koje Towalett skuplja. Sa svojih pet očiju traži tigove poput pamučnih štapića, duhana ili vlažnih maramica, a njima nije mjesto u zahodu!";
"trashmonster_wellmer_description" = "Mnogi krhki paketi dolaze u valovitom kartonu, zbog čega se Wellmer osjeća kao vitez koji štiti krhke stvari. Voli ponovno koristiti valoviti karton i često izrađuje viteške dvorce od starih kutija.";
"undp_goal" = "%@ za gol";
"undp_goal_10_description" = "To znači da gdje god živjeli u svijetu, trebali biste imati slična prava i mogućnosti kao i svi drugi.";
"undp_goal_10_title" = "Cilj 10: Smanjene nejednakosti";
"undp_goal_11_description" = "To znači da bi svatko trebao moći živjeti u sigurnim i dobrim domovima.";
"undp_goal_11_title" = "11. cilj: Održivi gradovi i kumuniteti";
"undp_goal_12_description" = "To znači da bismo trebali kupovati samo stvari koje su nam potrebne bez rasipanja ili bacanja previše stvari.";
"undp_goal_12_title" = "Cilj 12: Odgovorna potrošnja i proizvodnja";
"undp_goal_13_description" = "Da bismo dobro živjeli na našem planetu, moramo osigurati zaustavljanje klimatskih promjena.";
"undp_goal_13_title" = "13. cilj: Djelovanje u području klime";
"undp_goal_14_description" = "To znači da bismo se trebali pobrinuti za naše oceane bez bacanja smeća ili hvatanja previše ribe.";
"undp_goal_14_title" = "Cilj 14: Život ispod vode";
"undp_goal_15_description" = "To znači da se trebamo brinuti o našoj prirodi i svima koji tamo žive, i životinjama i ljudima, kao i biljkama i insecima.";
"undp_goal_15_title" = "Cilj 15: Život na kopnu";
"undp_goal_16_description" = "To znači da bismo trebali imati mirna i pravedna društva.";
"undp_goal_16_title" = "16. cilj: Mir, pravda i snažne institucije";
"undp_goal_17_description" = "Da bismo stvorili bolji svijet trebali bismo raditi zajedno, jedni s drugima i s drugim zemljama.";
"undp_goal_17_title" = "Cilj 17:Partnerstva za ciljeve";
"undp_goal_1_description" = "Siromaštvo može značiti da vam nedostaje hrane ili novca, ne osjećate se sigurno ili ne smijete ići u školu.";
"undp_goal_1_title" = "Cilj 1: Nema siromaštva";
"undp_goal_2_description" = "To znači da bi svatko trebao biti u stanju jesti hranu uzgojenu na dobar način i da nitko ne bi trebao gladovati.";
"undp_goal_2_title" = "Cilj 2: Nula gladi";
"undp_goal_3_description" = "To znači da bi svatko trebao moći živjeti zdrav i dobar život.";
"undp_goal_3_title" = "Cilj 3: Dobro zdravlje i dobrobit";
"undp_goal_4_description" = "To znači da bi svatko trebao moći ići u školu i imati priliku učiti nove stvari tijekom našeg života.";
"undp_goal_4_title" = "Cilj 4: Kvalitetno obrazovanje";
"undp_goal_5_description" = "To znači da su svi jednako vrijedni, i djevojčice i dječaci.";
"undp_goal_5_title" = "5. cilj: Rodna ravnopravnost";
"undp_goal_6_description" = "To znači da bi svatko trebao biti u stanju piti čistu vodu i moći ići na zahod.";
"undp_goal_6_title" = "Cilj 6: Čista voda i sanitarni uvjeti";
"undp_goal_7_description" = "To znači da bi svatko trebao imati struju i toplinu koja je dobra za naš planet.";
"undp_goal_7_title" = "Cilj 7: Pristupačna i čista energija";
"undp_goal_8_description" = "To znači da bi se svatko trebao moći osjećati dobro na poslu. Također ne biste trebali raditi cijeli dan, ali i moći se opustiti u slobodno vrijeme.";
"undp_goal_8_title" = "8. cilj: Dostojanstven rad i gospodarski rast";
"undp_goal_9_description" = "To znači da proizvodnja naših izuma mora biti zdrava i dobra za nas i našu prirodu.";
"undp_goal_9_title" = "Deveti cilj: Industrija, inovacije i infrastruktura";
