"trashmonster_batman_description" = "Batman adalah pria yang bermuatan. Ketika energinya mulai habis, dia melakukan perjalanan penemuan dan mencari baterai baru.";
"trashmonster_elli_description" = "Hal-hal listrik adalah favorit Ellis. Pengering rambut, tablet, dan ponsel - dia merawat semuanya";
"trashmonster_farao_description" = "Farao sedikit berbahaya. Dia marah seperti lebah ketika dia tidak menemukan limbah berbahayanya. Yang paling dia sukai adalah cat kuku kering tua dan semprotan rambut.";
"trashmonster_flamman_description" = "Api dan panas adalah dua hal yang membuat Flamman bersemangat. Dia mengumpulkan semua yang tidak diinginkan orang lain. Popok, kuas piring, dan dot, itu barangnya!";
"trashmonster_glas-klara_description" = "Glas-Klara berkilauan dan berkilauan seperti bola kaca halus seperti dia. Dengan matanya yang besar dan indah, dia memindai sekeliling dan mencari kaca yang tidak berwarna, yang merupakan hal terbaik yang dia tahu.";
"trashmonster_glasmo_description" = "Glasmo menyukai benda-benda kaca gelap tetapi takut rusak oleh pecahan kaca. Dengan enam kakinya yang sempit, dia menyelinap melalui tempat sampah dan wadah untuk menemukan botol dan toples terbaik.";
"trashmonster_kompostina_description" = "Kulit pisang, inti apel atau pasta, Kompostina mengumpulkan banyak hal dengan cara makanan. Dia tahu bahwa penting bagi kita untuk mengurangi limbah makanan kita, jadi dia sangat senang ketika dia menemukan sisa makanan yang sebenarnya tidak dapat dimakan.";
"trashmonster_lysien_description" = "Lysien benar-benar takut pada kegelapan, menurutnya dalam kegelapan ada monster yang berbahaya dan menakutkan. Di atas kepalanya dia memiliki lampu LED sendiri dan di tangannya dia selalu membawa lampu neon bersamanya. Dia menyalakan ini segera setelah kegelapan mulai turun.";
"trashmonster_metallika_description" = "Logam adalah yang terbaik yang diketahui Metallika. Dia mengumpulkan semua yang sulit disentuh dan mengeluarkan suara saat memukulnya, sama seperti dirinya sendiri!";
"trashmonster_nova-kras_description" = "Gelas minum pecah, barang pecah belah atau pot tanah liat, ini adalah hal-hal yang disukai Nova-Kras.";
"trashmonster_pantomen_description" = "Pantomen mengumpulkan semua kaleng dan botol yang memiliki tanda deposit di atasnya. Pantomen juga memiliki tanda pada dirinya, hanya agar dia tidak bercampur dengan Plastis atau Metallika.";
"trashmonster_pappis_description" = "Ada banyak kertas, yang bagus untuk Pappis yang suka mengoleksi kemasan kertas bekas. Paket Kellog, kotak sepatu, dan paket krim adalah beberapa hal favoritnya.";
"trashmonster_plastis_description" = "Kemasan plastik bagus untuk dimiliki, setidaknya menurut Plastis yang berpikir bahwa membuat penemuan dan mainan baru dari kemasan lama benar-benar menyenangkan!";
"trashmonster_prassel_description" = "Prassel bisa membaca koran untuk selamanya. Teka-teki silang yang belum terselesaikan, cerita lucu, dan berita kemarin - SEMUA yang ingin dia baca.";
"trashmonster_press_to_learn_more" = "Ketuk monster untuk mempelajarinya lebih lanjut.";
"trashmonster_stubben_description" = "Tunggul mengumpulkan daun, ranting dan bunga dan yang terbaik dari semuanya adalah bahwa apa yang ia kumpulkan menjadi tanah baru di mana Anda dapat menanam pohon atau bunga baru.";
"trashmonster_textilia_description" = "Textilia cukup baru di geng. Dia mengoleksi pakaian dan sepatu tua dan usang, tetapi juga pakaian yang bagus dan bisa digunakan lagi.";
"trashmonster_towalett_description" = "Kencing, kotoran, dan kertas toilet adalah satu-satunya hal yang dikumpulkan Towalett. Dengan kelima matanya dia mencari thigs seperti kapas, tembakau atau tisu basah, ini bukan milik toilet!";
"trashmonster_wellmer_description" = "Banyak paket rapuh datang dalam kardus bergelombang, itulah sebabnya Wellmer merasa seperti seorang ksatria yang melindungi hal-hal yang rapuh. Dia suka menggunakan kembali kardus bergelombang dan sering membuat kastil ksatria dari kotak-kotak tua.";
"undp_goal" = "%@ Tujuan";
"undp_goal_10_description" = "Ini berarti bahwa di mana pun Anda tinggal di dunia, Anda harus memiliki hak dan peluang yang sama seperti orang lain.";
"undp_goal_10_title" = "Tujuan 10: Mengurangi ketidaksetaraan";
"undp_goal_11_description" = "Ini berarti bahwa setiap orang harus dapat hidup di rumah yang aman dan baik.";
"undp_goal_11_title" = "Sasaran 11: Kota dan cummunities yang berkelanjutan";
"undp_goal_12_description" = "Ini berarti bahwa kita hanya boleh membeli barang-barang yang kita butuhkan tanpa membuang atau membuang terlalu banyak barang.";
"undp_goal_12_title" = "Sasaran 12: Konsumsi dan produksi yang bertanggung jawab";
"undp_goal_13_description" = "Untuk hidup dengan baik di planet kita, kita harus memastikan bahwa perubahan iklim dihentikan.";
"undp_goal_13_title" = "Sasaran 13: Aksi iklim";
"undp_goal_14_description" = "Ini berarti bahwa kita harus menjaga lautan kita tanpa membuang sampah sembarangan atau menangkap terlalu banyak ikan.";
"undp_goal_14_title" = "Tujuan 14: Hidup di bawah air";
"undp_goal_15_description" = "Ini berarti bahwa kita harus menjaga sifat kita dan semua orang yang tinggal di sana, baik hewan maupun manusia serta tumbuhan dan insetak.";
"undp_goal_15_title" = "Tujuan 15: Kehidupan di darat";
"undp_goal_16_description" = "Ini berarti bahwa kita harus memiliki masyarakat yang damai dan adil.";
"undp_goal_16_title" = "Tujuan 16: Perdamaian, keadilan, dan institusi yang kuat";
"undp_goal_17_description" = "Untuk menciptakan dunia yang lebih baik kita harus bekerja sama, dengan satu sama lain, dan dengan negara-negara lain.";
"undp_goal_17_title" = "Sasaran 17: Kemitraan untuk tujuan";
"undp_goal_1_description" = "Kemiskinan dapat berarti bahwa Anda kekurangan makanan atau uang, tidak merasa aman, atau tidak diizinkan pergi ke sekolah.";
"undp_goal_1_title" = "Tujuan 1: Tidak ada kemiskinan";
"undp_goal_2_description" = "Ini berarti bahwa setiap orang harus dapat makan makanan yang ditanam dengan cara yang baik, dan bahwa tidak ada yang harus kelaparan.";
"undp_goal_2_title" = "Sasaran 2: Tidak ada rasa lapar";
"undp_goal_3_description" = "Ini berarti bahwa setiap orang harus dapat menjalani kehidupan yang sehat dan baik.";
"undp_goal_3_title" = "Tujuan 3: Kesehatan dan kesejahteraan yang baik";
"undp_goal_4_description" = "Ini berarti bahwa setiap orang harus dapat pergi ke sekolah dan memiliki kesempatan untuk mempelajari hal-hal baru sepanjang hidup kita.";
"undp_goal_4_title" = "Tujuan 4: Pendidikan berkualitas";
"undp_goal_5_description" = "Ini berarti bahwa setiap orang sama-sama berharga, baik perempuan maupun laki-laki.";
"undp_goal_5_title" = "Sasaran 5: Kesetaraan gender";
"undp_goal_6_description" = "Artinya, setiap orang harus bisa minum air bersih dan bisa pergi ke toilet.";
"undp_goal_6_title" = "Sasaran 6: Air bersih dan sanitasi";
"undp_goal_7_description" = "Ini berarti bahwa setiap orang harus dapat memiliki listrik dan panas yang baik untuk planet kita.";
"undp_goal_7_title" = "Sasaran 7: Energi yang terjangkau dan bersih";
"undp_goal_8_description" = "Ini berarti bahwa setiap orang harus dapat merasa baik di tempat kerja. Anda juga tidak harus bekerja sepanjang hari, tetapi juga dapat bersantai di waktu luang Anda.";
"undp_goal_8_title" = "Tujuan 8: Pekerjaan yang layak dan pertumbuhan ekonomi";
"undp_goal_9_description" = "Ini berarti bahwa pembuatan penemuan kita harus sehat dan baik untuk kita dan sifat kita.";
"undp_goal_9_title" = "Sasaran 9: Industri, inovasi, dan infrastruktur";
