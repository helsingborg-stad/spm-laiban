"trashmonster_aqua-liv_description" = "ایکوا لیو وقت کی صبح سے موجود ہے، اور یہاں تک کہ ڈائنوسار اور وائکنگز سے ملا ہے! ایکوا لیو کوئی کچرا جمع نہیں کرتا لیکن دنیا کے تمام پانی کو کنٹرول کرتا ہے۔";
"trashmonster_batman_description" = "بیٹ مین ایک چارج ڈ آدمی ہے. جب اس کی توانائی ختم ہونا شروع ہو جاتی ہے تو وہ دریافت کے سفر پر جاتا ہے اور نئی بیٹریوں کی تلاش کرتا ہے۔";
"trashmonster_elli_description" = "برقی چیزیں ایلس کی پسندیدہ ہیں۔ ہیئر ڈرائر، ٹیبلٹ اور موبائل فون - وہ ان سب کی دیکھ بھال کرتی ہے";
"trashmonster_farao_description" = "فاراؤ تھوڑا خطرناک ہے۔ جب اسے اپنا خطرناک فضلہ نہیں ملتا تو وہ مکھی کی طرح غصے میں آجاتی ہے۔ وہ جو چیز سب سے زیادہ پسند کرتی ہے وہ پرانی خشک نیل پالش اور بالوں کا سپرے ہے۔";
"trashmonster_flamman_description" = "آگ اور گرمی دو چیزیں ہیں جن کے بارے میں فلامن پرجوش ہے۔ وہ وہ سب کچھ جمع کرتی ہے جو کوئی اور نہیں چاہتا۔ ڈائپرز، ڈش برش اور پیسیفائر، یہ اس کا سامان ہے!";
"trashmonster_glas-klara_description" = "گلاس کلارا شیشے کی باریک گیند کی طرح چمکتا اور چمکتا ہے جو وہ ہے۔ اپنی بڑی خوبصورت آنکھ سے، وہ ارد گرد کے ماحول کو سکین کرتی ہے اور بے رنگ شیشے کی تلاش کرتی ہے، جو بہترین چیزیں ہیں جو وہ جانتی ہے۔";
"trashmonster_glasmo_description" = "گلاسمو گہرے شیشے کی اشیاء کو پسند کرتا ہے لیکن ٹوٹے ہوئے شیشے سے خراب ہونے سے خوفزدہ ہے۔ اپنی چھ تنگ ٹانگوں کے ساتھ، وہ بہترین بوتلیں اور جار تلاش کرنے کے لئے کوڑے دان اور ڈبوں کے ذریعے چپکے سے جاتا ہے۔";
"trashmonster_kompostina_description" = "کیلے کا چھلکا، سیب کے کور یا پاستا، کومپوسٹینا کھانے کے طریقوں سے زیادہ تر چیزیں جمع کرتا ہے۔ وہ جانتی ہے کہ یہ ضروری ہے کہ ہم اپنے کھانے کے فضلے کو کم کریں، لہذا جب اسے بچا ہوا کھانا ملتا ہے جو اصل میں کھانے کے قابل نہیں ہوتا تو وہ زیادہ خوش ہوتی ہے۔";
"trashmonster_lysien_description" = "لائسین واقعی اندھیرے سے خوفزدہ ہے، وہ سمجھتا ہے کہ اندھیرے میں خطرناک اور خوفناک عفریت ہیں۔ اس کے سر کے اوپر اس کا اپنا ایل ای ڈی لیمپ ہے اور اس کے ہاتھ میں وہ ہمیشہ اپنے ساتھ فلوریسنٹ لیمپ لے جاتا ہے۔ جیسے ہی اندھیرا گرنا شروع ہوتا ہے وہ ان کو روشن کرتا ہے۔";
"trashmonster_metallika_description" = "دھات بہترین میٹلیکا جانتی ہے۔ وہ وہ سب کچھ اکٹھا کرتا ہے جسے چھونا مشکل ہے اور جب اسے مارا جاتا ہے تو آوازیں نکالتا ہے، بالکل اپنی طرح!";
"trashmonster_nova-kras_description" = "ٹوٹے ہوئے پینے کے گلاس، کراکری یا مٹی کے برتن، یہ وہ چیزیں ہیں جو نووا کراس کو پسند ہیں۔";
"trashmonster_pantomen_description" = "پینٹومین وہ تمام ڈبے اور بوتلیں جمع کرتا ہے جن پر ڈپازٹ کا نشان ہوتا ہے۔ پینٹومین بھی اس پر ایک نشان ہے، صرف اس لئے کہ وہ پلاسٹیس یا میٹلیکا کے ساتھ نہیں ملایا جائے گا.";
"trashmonster_pappis_description" = "کاغذ کی بہتات ہے، جو پپیس کے لئے اچھا ہے جو پرانے کاغذ کی پیکیجنگ جمع کرنا پسند کرتا ہے۔ کیلوگ پیکجز، جوتوں کے ڈبے اور کریم پیکجز اس کی پسندیدہ چیزوں میں سے کچھ ہیں۔";
"trashmonster_plastis_description" = "پلاسٹک کی پیکیجنگ کے لئے اچھا ہے, کم از کم پلاسٹس کے مطابق جو سوچتا ہے کہ پرانی پیکیجنگ سے ایجادات اور نئے کھلونے بنانے واقعی مزہ ہے!";
"trashmonster_prassel_description" = "پرسل ہمیشہ کے لئے اخبارات پڑھ سکتا تھا۔ غیر حل شدہ کراس ورڈ پہیلیاں، مضحکہ خیز کہانیاں اور کل کی خبریں - وہ سب کچھ جو وہ پڑھنا چاہتی ہے۔";
"trashmonster_press_to_learn_more" = "اس کے بارے میں مزید جاننے کے لئے کسی عفریت کو چھوئیں۔";
"trashmonster_stubben_description" = "اسٹمپ پتے، ٹہنیاں اور پھول جمع کرتا ہے اور سب سے اچھی بات یہ ہے کہ جو کچھ وہ جمع کرتا ہے وہ نئی مٹی بن جاتا ہے جس میں آپ نئے درخت یا پھول لگا سکتے ہیں۔";
"trashmonster_textilia_description" = "ٹیکسٹیلیا گینگ کے لئے بہت نیا ہے۔ وہ پرانے اور پہنے ہوئے کپڑے اور جوتے جمع کرتی ہے، لیکن ایسے لباس بھی جمع کرتی ہے جو اچھے ہیں اور دوبارہ استعمال کیے جا سکتے ہیں۔";
"trashmonster_towalett_description" = "پیشاب، پوپ اور ٹوائلٹ پیپر ہی وہ چیزیں ہیں جو ٹووالیٹ جمع کرتی ہیں۔ اپنی پانچ آنکھوں سے وہ کپاس کے پھوڑے، تمباکو یا گیلے وائپس جیسے تھیگ تلاش کرتی ہے، یہ ٹوائلٹ سے تعلق نہیں رکھتے!";
"trashmonster_wellmer_description" = "بہت سے نازک پیکجز نالی دار گتے میں آتے ہیں، یہی وجہ ہے کہ ویلمر ایک نائٹ کی طرح محسوس ہوتا ہے جو نازک چیزوں کی حفاظت کرتا ہے۔ وہ نالی دار گتے کو دوبارہ استعمال کرنا پسند کرتا ہے اور اکثر پرانے ڈبوں سے نائٹس کے قلعے بناتا ہے۔";
"undp_goal" = "ہدف %@";
