"trashmonster_aqua-liv_description" = "Aqua-Liv đã tồn tại từ buổi bình minh của thời gian, và thậm chí đã gặp khủng long và Vikings! Aqua-Liv không thu gom bất kỳ rác nào nhưng kiểm soát tất cả nước trên thế giới.";
"trashmonster_batman_description" = "Batman là một gã bị buộc tội. Khi năng lượng của anh ta bắt đầu cạn kiệt, anh ta đi trên một hành trình khám phá và tìm kiếm pin mới.";
"trashmonster_elli_description" = "Những thứ điện là yêu thích của Ellis. Máy sấy tóc, máy tính bảng và điện thoại di động - cô ấy chăm sóc tất cả chúng";
"trashmonster_farao_description" = "Farao hơi nguy hiểm. Cô ấy tức giận như một con ong khi cô ấy không tìm thấy chất thải nguy hại của mình. Điều cô yêu thích nhất là sơn móng tay khô cũ và xịt tóc.";
"trashmonster_flamman_description" = "Lửa và nhiệt là hai điều mà Flamman đam mê. Cô ấy thu thập tất cả mọi thứ mà không ai khác muốn. Tã lót, bàn chải đĩa và núm vú giả, đó là công cụ của cô ấy!";
"trashmonster_glas-klara_description" = "Glas-Klara lấp lánh và lung linh như quả bóng thủy tinh mịn màng. Với đôi mắt to đẹp của mình, cô quét xung quanh và tìm kiếm kính không màu, đó là những điều tốt nhất mà cô biết.";
"trashmonster_glasmo_description" = "Glasmo thích các vật dụng thủy tinh tối màu nhưng sợ bị hư hỏng bởi kính vỡ. Với sáu chân hẹp, anh lẻn qua thùng rác và thùng chứa để tìm những chai và lọ tốt nhất.";
"trashmonster_kompostina_description" = "Vỏ chuối, lõi táo hoặc mì ống, Kompostina thu thập hầu hết mọi thứ theo cách thức ăn. Cô ấy biết rằng điều quan trọng là chúng ta phải giảm lãng phí thực phẩm, vì vậy cô ấy rất hạnh phúc khi cô ấy tìm thấy thức ăn thừa không thực sự ăn được.";
"trashmonster_lysien_description" = "Lysien thực sự sợ bóng tối, anh nghĩ rằng trong bóng tối có những con quái vật nguy hiểm và đáng sợ. Trên đỉnh đầu, anh ta có đèn LED của riêng mình và trong tay anh ta luôn mang theo một chiếc đèn huỳnh quang. Anh ta thắp sáng những thứ này ngay khi bóng tối bắt đầu buông xuống.";
"trashmonster_metallika_description" = "Kim loại là kim loại tốt nhất biết. Anh ta thu thập mọi thứ khó chạm vào và tạo ra âm thanh khi nhấn nó, giống như chính anh ta!";
"trashmonster_nova-kras_description" = "Ly uống nước bị hỏng, đồ sành sứ hoặc nồi đất sét, đây là những thứ mà Nova-Kras thích.";
"trashmonster_pantomen_description" = "Pantomen thu thập tất cả các lon và chai có dấu ký gửi trên chúng. Pantomen cũng có một dấu ấn trên cô ấy, chỉ để cô ấy sẽ không bị trộn lẫn với Plastis hoặc Metallika.";
"trashmonster_pappis_description" = "Có rất nhiều giấy, điều này rất tốt cho Pappis, những người thích thu thập bao bì giấy cũ. Gói Kellog, hộp giày và gói kem là một số thứ yêu thích của anh ấy.";
"trashmonster_plastis_description" = "Bao bì nhựa là tốt để có, ít nhất là theo Plastis, người nghĩ rằng việc tạo ra các phát minh và đồ chơi mới từ bao bì cũ thực sự thú vị!";
"trashmonster_prassel_description" = "Prassel có thể đọc báo vĩnh viễn. Câu đố ô chữ chưa được giải quyết, những câu chuyện hài hước và tin tức ngày hôm qua - MỌI THỨ cô ấy muốn đọc.";
"trashmonster_press_to_learn_more" = "Chạm vào một con quái vật để tìm hiểu thêm về nó.";
"trashmonster_stubben_description" = "Gốc cây thu thập lá, cành cây và hoa và tốt nhất là những gì anh ta thu thập trở thành đất mới trong đó bạn có thể trồng cây hoặc hoa mới.";
"trashmonster_textilia_description" = "Textilia khá mới đối với băng đảng. Cô thu thập quần áo và giày cũ và mòn, nhưng cũng có quần áo đẹp và có thể được sử dụng lại.";
"trashmonster_towalett_description" = "Nước tiểu, phân và giấy vệ sinh là những thứ duy nhất Towalett thu thập được. Với năm mắt, cô tìm kiếm những miếng bánh như tăm bông, thuốc lá hoặc khăn ướt, chúng không thuộc về nhà vệ sinh!";
"trashmonster_wellmer_description" = "Nhiều gói mỏng manh đi kèm trong các tông sóng, đó là lý do tại sao Wellmer cảm thấy như một hiệp sĩ bảo vệ những thứ mong manh. Ông thích tái sử dụng các tông sóng và thường làm lâu đài của các hiệp sĩ từ các hộp cũ.";
"undp_goal" = "Mục tiêu %@";
"undp_goal_10_description" = "Điều này có nghĩa là bất cứ nơi nào bạn sống trên thế giới, bạn nên có các quyền và cơ hội tương tự như mọi người khác.";
"undp_goal_10_title" = "Mục tiêu 10: Giảm bất bình đẳng";
"undp_goal_11_description" = "Điều này có nghĩa là mọi người đều có thể sống trong những ngôi nhà an toàn và tốt đẹp.";
"undp_goal_11_title" = "Mục tiêu 11: Thành phố bền vững và kiêm nhiệm";
"undp_goal_12_description" = "Điều này có nghĩa là chúng ta chỉ nên mua những thứ chúng ta cần mà không lãng phí hoặc vứt bỏ quá nhiều thứ.";
"undp_goal_12_title" = "Mục tiêu 12: Tiêu dùng và sản xuất có trách nhiệm";
"undp_goal_13_description" = "Để sống tốt trên hành tinh của chúng ta, chúng ta phải đảm bảo rằng biến đổi khí hậu được ngăn chặn.";
"undp_goal_13_title" = "Mục tiêu 13: Hành động khí hậu";
"undp_goal_14_description" = "Điều này có nghĩa là chúng ta nên chăm sóc đại dương của chúng ta mà không xả rác hoặc bắt quá nhiều cá.";
"undp_goal_14_title" = "Mục tiêu 14: Cuộc sống dưới nước";
"undp_goal_15_description" = "Điều này có nghĩa là chúng ta nên chăm sóc bản chất của chúng ta và tất cả những người sống ở đó, cả động vật và con người cũng như thực vật và insetc.";
"undp_goal_15_title" = "Mục tiêu 15: Cuộc sống trên đất liền";
"undp_goal_16_description" = "Điều này có nghĩa là chúng ta nên có xã hội hòa bình và công bằng.";
"undp_goal_16_title" = "Mục tiêu 16: Hòa bình, công lý và các thể chế mạnh mẽ";
"undp_goal_17_description" = "Để tạo ra một thế giới tốt đẹp hơn, chúng ta nên làm việc cùng nhau, với nhau và với các quốc gia khác.";
"undp_goal_17_title" = "Mục tiêu 17: Quan hệ đối tác cho các mục tiêu";
"undp_goal_1_description" = "Nghèo đói có thể có nghĩa là bạn thiếu thức ăn hoặc tiền bạc, không cảm thấy an toàn hoặc không được phép đến trường.";
"undp_goal_1_title" = "Mục tiêu 1: Không nghèo";
"undp_goal_2_description" = "Điều này có nghĩa là mọi người đều có thể ăn thức ăn được trồng một cách tốt, và không ai phải đói.";
"undp_goal_2_title" = "Bàn thắng 2: Không đói";
"undp_goal_3_description" = "Điều này có nghĩa là mọi người đều có thể sống một cuộc sống lành mạnh và tốt đẹp.";
"undp_goal_3_title" = "Mục tiêu 3: Sức khỏe và hạnh phúc tốt";
"undp_goal_4_description" = "Điều này có nghĩa là mọi người đều có thể đi học và có cơ hội học hỏi những điều mới trong suốt cuộc đời của chúng ta.";
"undp_goal_4_title" = "Mục tiêu 4: Chất lượng giáo dục";
"undp_goal_5_description" = "Điều này có nghĩa là mọi người đều có giá trị như nhau, cả bé gái và bé trai.";
"undp_goal_5_title" = "Mục tiêu 5: Bình đẳng giới";
"undp_goal_6_description" = "Điều này có nghĩa là mọi người đều có thể uống nước sạch và có thể đi vệ sinh.";
"undp_goal_6_title" = "Mục tiêu 6: Nước sạch và vệ sinh môi trường";
"undp_goal_7_description" = "Điều này có nghĩa là mọi người đều có thể có điện và nhiệt tốt cho hành tinh của chúng ta.";
"undp_goal_7_title" = "Mục tiêu 7: Năng lượng sạch và giá cả phải chăng";
"undp_goal_8_description" = "Điều này có nghĩa là mọi người đều có thể cảm thấy tốt trong công việc. Bạn cũng không phải làm việc cả ngày, nhưng cũng có thể thư giãn trong thời gian rảnh rỗi.";
"undp_goal_8_title" = "Mục tiêu 8: Việc làm tốt và tăng trưởng kinh tế";
"undp_goal_9_description" = "Điều này có nghĩa là việc sản xuất các phát minh của chúng ta phải lành mạnh và tốt cho chúng ta và bản chất của chúng ta.";
