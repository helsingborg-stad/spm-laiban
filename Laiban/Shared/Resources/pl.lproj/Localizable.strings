"trashmonster_aqua-liv_description" = "Aqua-Liv istnieje od zarania dziejów, a nawet spotkał dinozaury i Wikingów! Aqua-Liv nie zbiera żadnych śmieci, ale kontroluje całą wodę na świecie.";
"trashmonster_batman_description" = "Batman jest naładowanym facetem. Kiedy jego energia zaczyna się wyczerpywać, wyrusza w podróż odkrywczą i szuka nowych baterii.";
"trashmonster_elli_description" = "Elektryczne rzeczy są ulubionymi rzeczami Ellisa. Suszarki do włosów, tablety i telefony komórkowe - dba o nie wszystkie";
"trashmonster_farao_description" = "Farao jest trochę niebezpieczne. Złości się jak pszczoła, gdy nie znajduje swoich niebezpiecznych odpadów. To, co kocha najbardziej, to stary suszony lakier do paznokci i lakier do włosów.";
"trashmonster_flamman_description" = "Ogień i ciepło to dwie rzeczy, które pasjonują Flammana. Zbiera wszystko, czego nikt inny nie chce. Pieluchy, szczotki do naczyń i smoczki, to jej rzeczy!";
"trashmonster_glas-klara_description" = "Glas-Klara błyszczy i migocze jak drobna kula szkła, którą jest. Swoim wielkim, pięknym okiem skanuje otoczenie i szuka bezbarwnego szkła, które są najlepszymi rzeczami, jakie zna.";
"trashmonster_glasmo_description" = "Glasmo uwielbia przedmioty z ciemnego szkła, ale boi się, że zostanie uszkodzony przez potłuczone szkło. Z sześcioma wąskimi nogami przemyka przez kosze na śmieci i pojemniki, aby znaleźć najlepsze butelki i słoiki.";
"trashmonster_kompostina_description" = "Skórka od banana, rdzenie jabłek lub makaron, Kompostina zbiera większość rzeczy w sposób spożywczy. Wie, że ważne jest, abyśmy ograniczyli marnowanie żywności, więc jest bardzo szczęśliwa, gdy znajdzie resztki, które w rzeczywistości nie są jadalne.";
"trashmonster_lysien_description" = "Lysien naprawdę boi się ciemności, myśli, że w ciemności są niebezpieczne i przerażające. Na czubku głowy ma własną lampę LED, a w ręku zawsze nosi ze sobą lampę fluorescencyjną. Zapala je, gdy tylko ciemność zacznie padać.";
"trashmonster_metallika_description" = "Metal jest najlepszym, co zna Metallika. Zbiera wszystko, co jest trudne do dotknięcia i wydaje dźwięki po uderzeniu, tak jak on sam!";
"trashmonster_nova-kras_description" = "Zepsute szklanki do picia, naczynia lub gliniane garnki, to rzeczy, które Nova-Kras lubi.";
"trashmonster_pantomen_description" = "Pantomen zbiera wszystkie puszki i butelki, które mają na nich znak depozytu. Pantomen ma również na sobie znak, tylko po to, aby nie pomylić się z Plastis lub Metalliką.";
"trashmonster_pappis_description" = "Jest dużo papieru, co jest dobre dla Pappis, który lubi zbierać stare papierowe opakowania. Opakowania Kelloga, pudełka na buty i opakowania kremów to tylko niektóre z jego ulubionych rzeczy.";
"trashmonster_plastis_description" = "Plastikowe opakowania dobrze jest mieć, przynajmniej według Plastisa, który uważa, że tworzenie wynalazków i nowych zabawek ze starych opakowań jest naprawdę zabawne!";
"trashmonster_prassel_description" = "Prassel mógł czytać gazety przez wieczność. Nierozwiązane krzyżówki, zabawne historie i wczorajsze wiadomości - WSZYSTKO, co chce przeczytać.";
"trashmonster_press_to_learn_more" = "Stuknij, aby dowiedzieć się więcej na jego temat.";
"trashmonster_stubben_description" = "Kikut zbiera liście, gałązki i kwiaty, a najlepsze jest to, że to, co zbiera, staje się nową glebą, w której można sadzić nowe drzewa lub kwiaty.";
"trashmonster_textilia_description" = "Textilia jest całkiem nowa w gangu. Zbiera stare i zużyte ubrania i buty, ale także ubrania, które są ładne i mogą być używane ponownie.";
"trashmonster_towalett_description" = "Siusiu, kupy i papieru toaletowego to jedyne, co zbiera Towalett. Z pięciorgiem oczu szuka thigs, takich jak bawełniane waciki, tytoń lub mokre chusteczki, które nie należą do toalety!";
"trashmonster_wellmer_description" = "Wiele delikatnych paczek jest w tekturze falistej, dlatego Wellmer czuje się jak rycerz, który chroni delikatne rzeczy. Uwielbia ponownie używać tektury falistej i często robi zamki rycerskie ze starych pudełek.";
"undp_goal" = "%@ celu";
"undp_goal_10_description" = "Oznacza to, że gdziekolwiek mieszkasz na świecie, powinieneś mieć podobne prawa i możliwości, jak wszyscy inni.";
"undp_goal_10_title" = "Cel 10: Zmniejszenie nierówności";
"undp_goal_11_description" = "Oznacza to, że każdy powinien móc mieszkać w bezpiecznych i dobrych domach.";
"undp_goal_11_title" = "Cel 11: Zrównoważone miasta i kumuny";
"undp_goal_12_description" = "Oznacza to, że powinniśmy kupować tylko rzeczy, których potrzebujemy, bez marnowania lub wyrzucania zbyt wielu rzeczy.";
"undp_goal_12_title" = "Cel 12: Odpowiedzialna konsumpcja i produkcja";
"undp_goal_13_description" = "Aby dobrze żyć na naszej planecie, musimy zapewnić powstrzymanie zmian klimatycznych.";
"undp_goal_13_title" = "Cel 13: Działania w dziedzinie klimatu";
"undp_goal_14_description" = "Oznacza to, że powinniśmy dbać o nasze oceany bez zaśmiecania czy łowienia zbyt dużej ilości ryb.";
"undp_goal_14_title" = "Cel 14: Życie pod wodą";
"undp_goal_15_description" = "Oznacza to, że powinniśmy dbać o naszą przyrodę i wszystkich, którzy tam mieszkają, zarówno zwierzęta, jak i ludzi, a także rośliny i insetki.";
"undp_goal_15_title" = "Cel 15: Życie na lądzie";
"undp_goal_16_description" = "Oznacza to, że powinniśmy mieć pokojowe i sprawiedliwe społeczeństwa.";
"undp_goal_16_title" = "Cel 16: Pokój, sprawiedliwość i silne instytucje";
"undp_goal_17_description" = "Aby stworzyć lepszy świat, powinniśmy pracować razem, ze sobą nawzajem i z innymi krajami.";
"undp_goal_17_title" = "Cel 17: Partnerstwo na rzecz celów";
"undp_goal_1_description" = "Ubóstwo może oznaczać, że brakuje ci jedzenia lub pieniędzy, nie czujesz się bezpiecznie lub nie możesz chodzić do szkoły.";
"undp_goal_1_title" = "Cel 1: Brak ubóstwa";
"undp_goal_2_description" = "Oznacza to, że każdy powinien mieć możliwość jedzenia wyhodowanego w dobry sposób i że nikt nie powinien być głodny.";
"undp_goal_2_title" = "Cel 2: Zero głodu";
"undp_goal_3_description" = "Oznacza to, że każdy powinien być w stanie żyć zdrowo i dobrze.";
"undp_goal_3_title" = "Cel 3: Dobre zdrowie i dobre samopoczucie";
"undp_goal_4_description" = "Oznacza to, że każdy powinien mieć możliwość chodzenia do szkoły i mieć możliwość uczenia się nowych rzeczy przez całe życie.";
"undp_goal_4_title" = "Cel 4: Wysokiej jakości edukacja";
"undp_goal_5_description" = "Oznacza to, że wszyscy są równie wartościowi, zarówno dziewczęta, jak i chłopcy.";
"undp_goal_5_title" = "Cel 5: Równość płci";
"undp_goal_6_description" = "Oznacza to, że każdy powinien mieć możliwość picia czystej wody i być w stanie iść do toalety.";
"undp_goal_6_title" = "Cel 6: Czysta woda i urządzenia sanitarne";
"undp_goal_7_description" = "Oznacza to, że każdy powinien mieć możliwość posiadania elektryczności i ciepła, które są dobre dla naszej planety.";
"undp_goal_7_title" = "Cel 7: Przystępna cenowo i czysta energia";
"undp_goal_8_description" = "Oznacza to, że każdy powinien być w stanie czuć się dobrze w pracy. Nie powinieneś też pracować cały dzień, ale także móc odpocząć w wolnym czasie.";
"undp_goal_8_title" = "Cel 8: Godna praca i wzrost gospodarczy";
"undp_goal_9_description" = "Oznacza to, że produkcja naszych wynalazków musi być zdrowa i dobra dla nas i naszej natury.";
"undp_goal_9_title" = "Cel 9: Przemysł, innowacje i infrastruktura";
