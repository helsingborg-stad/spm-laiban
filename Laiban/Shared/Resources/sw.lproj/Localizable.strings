"trashmonster_flamman_description" = "Moto na joto ni vitu viwili ambavyo Flamman anapenda sana. Anakusanya kila kitu ambacho hakuna mtu mwingine anayetaka. Diapers, brashi za sahani na pacifiers, hayo ni mambo yake!";
"trashmonster_glas-klara_description" = "Glas-Klara glimmers na shimmers kama mpira mzuri wa kioo alicho. Kwa jicho lake kubwa zuri, anachanganua mazingira na kutafuta kioo kisicho na rangi, ambayo ni vitu bora zaidi ambavyo anajua.";
"trashmonster_glasmo_description" = "Glasmo anapenda vitu vyeusi vya kioo lakini anaogopa kuharibiwa na kioo kilichovunjika. Akiwa na miguu yake sita myembamba, anapitia makopo ya takataka na vyombo ili kupata chupa na mitungi mizuri zaidi.";
"trashmonster_kompostina_description" = "Ndizi peel, apple cores au tambi, Kompostina hukusanya vitu vingi kwa njia za chakula. Anajua kwamba ni muhimu tupunguze uharibifu wetu wa chakula, hivyo anafurahi zaidi anapopata mabaki ambayo kwa kweli hayaliwi.";
"trashmonster_lysien_description" = "Lysien anaogopa sana giza, anafikiri kwamba gizani kuna monsters hatari na za kutisha. Juu ya kichwa chake ana taa yake ya LED na mkononi mwake kila wakati hubeba taa ya fluorescent pamoja naye. Anawasha hizi mara tu giza linapoanza kuanguka.";
"trashmonster_metallika_description" = "Metali ni Metallika bora anajua. Anakusanya kila kitu ambacho ni vigumu kugusa na kutoa sauti anapokipiga, kama yeye mwenyewe!";
"trashmonster_nova-kras_description" = "Miwani ya kunywa iliyovunjika, vyungu vya udongo au udongo, hivi ni vitu ambavyo Nova-Kras anapenda.";
"trashmonster_pantomen_description" = "Pantomen hukusanya makopo na chupa zote ambazo zina alama ya amana juu yao. Pantomen pia ana alama juu yake, kwa hivyo tu hatachanganywa na Plastis au Metallika.";
"trashmonster_pappis_description" = "Kuna karatasi nyingi, ambayo ni nzuri kwa Pappis ambaye anapenda kukusanya vifungashio vya karatasi za zamani. Vifurushi vya Kellog, masanduku ya viatu na vifurushi vya krimu ni baadhi ya vitu anavyovipenda.";
"trashmonster_plastis_description" = "Vifungashio vya plastiki ni vizuri kuwa navyo, angalau kulingana na Plastis ambaye anafikiri kwamba kuunda uvumbuzi na vinyago vipya kutoka kwa vifungashio vya zamani ni furaha sana!";
"trashmonster_prassel_description" = "Prassel aliweza kusoma magazeti kwa milele. Mafumbo ya msalaba yasiyotatuliwa, hadithi za kuchekesha na habari za jana - KILA KITU anachotaka kusoma.";
"trashmonster_press_to_learn_more" = "Gusa monster ili ujifunze zaidi juu yake.";
"trashmonster_stubben_description" = "Mkonge hukusanya majani, majani na maua na bora zaidi ni kwamba anachokusanya kinakuwa udongo mpya ambao unaweza kupanda miti mipya au maua.";
"trashmonster_textilia_description" = "Textilia ni mpya sana kwa genge. Anakusanya nguo za zamani na zilizovaliwa na viatu, lakini pia mavazi ambayo ni mazuri na yanaweza kutumika tena.";
"trashmonster_towalett_description" = "Pee, poop na karatasi ya choo ndio vitu pekee ambavyo Towalett hukusanya. Kwa macho yake matano anatafuta majambazi kama mapambano ya pamba, tumbaku au ufuta mvua, haya siyo ya chooni!";
"trashmonster_wellmer_description" = "Vifurushi vingi dhaifu huja katika kadi iliyoharibika, ndiyo sababu Wellmer anahisi kama knight ambaye analinda vitu dhaifu. Anapenda kutumia tena kadi iliyoharibika na mara nyingi hufanya majumba ya knights nje ya masanduku ya zamani.";
"undp_goal" = "Lengo %@";
"undp_goal_10_description" = "Hii ina maana kwamba popote unapoishi duniani, unapaswa kuwa na haki na fursa zinazofanana kama watu wengine.";
"undp_goal_10_title" = "Lengo 10: Kupunguza ukosefu wa usawa";
"undp_goal_11_description" = "Hii ina maana kwamba kila mtu anapaswa kuishi katika nyumba salama na nzuri.";
"undp_goal_11_title" = "Lengo la 11: Miji endelevu na cummunities";
"undp_goal_12_description" = "Hii ina maana kwamba tunapaswa kununua tu vitu tunavyohitaji bila kupoteza au kutupa vitu vingi sana.";
"undp_goal_12_title" = "Lengo namba 12: Matumizi ya uwajibikaji na uzalishaji";
"undp_goal_13_description" = "Ili kuishi vizuri kwenye sayari yetu, lazima tuhakikishe kuwa mabadiliko ya hali ya hewa yanakomeshwa.";
"undp_goal_13_title" = "Lengo la 13: Hatua za hali ya hewa";
"undp_goal_14_description" = "Hii ina maana kwamba tunapaswa kutunza bahari zetu bila takataka au kukamata samaki wengi.";
"undp_goal_14_title" = "Lengo la 14: Maisha chini ya maji";
"undp_goal_15_description" = "Hii ina maana kwamba tunapaswa kutunza asili yetu na kila mtu anayeishi huko, wanyama na binadamu pamoja na mimea na insetcs.";
"undp_goal_15_title" = "Lengo la 15: Maisha juu ya ardhi";
"undp_goal_16_description" = "Hii ina maana kwamba tunapaswa kuwa na jamii zenye amani na haki.";
"undp_goal_16_title" = "Lengo 16: Amani, haki na taasisi imara";
"undp_goal_17_description" = "Ili kujenga dunia bora tunapaswa kufanya kazi pamoja, na kila mmoja, na nchi nyingine.";
"undp_goal_17_title" = "Lengo 17: Ushirikiano kwa malengo";
"undp_goal_1_description" = "Umaskini unaweza kumaanisha kuwa unakosa chakula au pesa, usijisikie salama, au huruhusiwi kwenda shule.";
"undp_goal_1_title" = "Lengo 1: Hakuna umaskini";
"undp_goal_2_description" = "Hii ina maana kwamba kila mtu anapaswa kula chakula kilichopandwa kwa njia nzuri, na kwamba hakuna mtu anayepaswa kulala njaa.";
"undp_goal_2_title" = "Lengo 2: Njaa sifuri";
"undp_goal_3_description" = "Hii ina maana kwamba kila mtu anapaswa kuishi maisha mazuri na mazuri.";
"undp_goal_3_title" = "Lengo la 3: Afya njema na ustawi";
"undp_goal_4_description" = "Hii ina maana kwamba kila mtu anapaswa kwenda shule na kupata fursa ya kujifunza mambo mapya katika maisha yetu yote.";
"undp_goal_4_title" = "Lengo la 4: Elimu bora";
"undp_goal_5_description" = "Hii ina maana kwamba kila mtu ana thamani sawa, wasichana na wavulana.";
"undp_goal_5_title" = "Lengo la 5: Usawa wa kijinsia";
"undp_goal_6_description" = "Hii ina maana kwamba kila mtu anapaswa kunywa maji safi na kuweza kwenda chooni.";
"undp_goal_6_title" = "Lengo la 6: Maji safi na usafi wa mazingira";
"undp_goal_7_description" = "Hii ina maana kwamba kila mtu anapaswa kuwa na umeme na joto ambalo ni zuri kwa sayari yetu.";
"undp_goal_7_title" = "Lengo la 7: Nishati nafuu na safi";
"undp_goal_8_description" = "Hii ina maana kwamba kila mtu anapaswa kujisikia vizuri kazini. Pia haupaswi kufanya kazi siku nzima, lakini pia uweze kupumzika katika muda wako wa bure.";
"undp_goal_8_title" = "Lengo la 8: Kazi nzuri na ukuaji wa uchumi";
"undp_goal_9_description" = "Hii ina maana kwamba utengenezaji wa uvumbuzi wetu lazima uwe na afya na nzuri kwetu na asili yetu.";
"undp_goal_9_title" = "Lengo 9: Viwanda, ubunifu na miundombinu";
