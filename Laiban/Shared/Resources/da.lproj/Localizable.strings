"undp_goal_10_title" = "Mål 10: Mindskede uligheder";
"undp_goal_11_description" = "Det betyder, at alle skal kunne bo i sikre og gode hjem.";
"undp_goal_11_title" = "Mål 11: Bæredygtige byer og kommuner";
"undp_goal_12_description" = "Det betyder, at vi kun skal købe ting, vi har brug for, uden at spilde eller smide for mange ting væk.";
"undp_goal_12_title" = "Mål 12: Ansvarligt forbrug og produktion";
"undp_goal_13_description" = "For at leve godt på vores planet skal vi sikre, at klimaændringerne stoppes.";
"undp_goal_13_title" = "Mål 13: Klimaindsats";
"undp_goal_14_description" = "Det betyder, at vi skal tage os af vores have uden at smide eller fange for meget fisk.";
"undp_goal_14_title" = "Mål 14: Livet under vandet";
"undp_goal_15_description" = "Det betyder, at vi skal passe på vores natur og alle, der bor der, både dyr og mennesker samt planter og insetcs.";
"undp_goal_15_title" = "Mål 15: Livet på land";
"undp_goal_16_description" = "Det betyder, at vi skal have fredelige og retfærdige samfund.";
"undp_goal_16_title" = "Mål 16: Fred, retfærdighed og stærke institutioner";
"undp_goal_17_description" = "For at skabe en bedre verden bør vi arbejde sammen, med hinanden og med andre lande.";
"undp_goal_17_title" = "Mål 17: Partnerskaber for målene";
"undp_goal_1_description" = "Fattigdom kan betyde, at du mangler mad eller penge, ikke føler dig sikker eller ikke får lov til at gå i skole.";
"undp_goal_1_title" = "Mål 1: Ingen fattigdom";
"undp_goal_2_description" = "Det betyder, at alle skal kunne spise mad, der dyrkes på en god måde, og at ingen skal sulte.";
"undp_goal_2_title" = "Mål 2: Nul sult";
"undp_goal_3_description" = "Det betyder, at alle skal kunne leve et sundt og godt liv.";
"undp_goal_3_title" = "Mål 3: Godt helbred og trivsel";
"undp_goal_4_description" = "Det betyder, at alle skal kunne gå i skole og få mulighed for at lære nye ting gennem hele vores liv.";
"undp_goal_4_title" = "Mål 4: Kvalitetsuddannelse";
"undp_goal_5_description" = "Det betyder, at alle er lige værdifulde, både piger og drenge.";
"undp_goal_5_title" = "Mål 5: Ligestilling mellem kønnene";
"undp_goal_6_description" = "Det betyder, at alle skal kunne drikke rent vand og være i stand til at gå på toilettet.";
"undp_goal_6_title" = "Mål 6: Rent vand og sanitet";
"undp_goal_7_description" = "Det betyder, at alle skal kunne have elektricitet og varme, der er godt for vores planet.";
"undp_goal_7_title" = "Mål 7: Økonomisk overkommelig og ren energi";
"undp_goal_8_description" = "Det betyder, at alle skal kunne have det godt på arbejdet. Du bør heller ikke skulle arbejde hele dagen, men også være i stand til at slappe af i din fritid.";
"undp_goal_8_title" = "Mål 8: Anstændigt arbejde og økonomisk vækst";
"undp_goal_9_description" = "Det betyder, at fremstillingen af vores opfindelser skal være sund og god for os og vores natur.";
"undp_goal_9_title" = "Mål 9: Industri, innovation og infrastruktur";
"undp_press_to_learn_more" = "Tryk på et mål for at få mere at vide om det.";
