"trashmonster_aqua-liv_description" = "एक्वा-लिव समय की सुबह से अस्तित्व में है, और यहां तक कि डायनासोर और वाइकिंग्स से भी मिला है! एक्वा-लिव कोई कचरा इकट्ठा नहीं करता है लेकिन दुनिया के सभी पानी को नियंत्रित करता है।";
"trashmonster_batman_description" = "बैटमैन एक चार्ज आदमी है। जब उसकी ऊर्जा खत्म होने लगती है, तो वह खोज की यात्रा पर जाता है और नई बैटरी की तलाश करता है।";
"trashmonster_elli_description" = "बिजली की चीजें एलिस की पसंदीदा हैं। हेयर ड्रायर, टैबलेट और सेल फोन - वह उन सभी का ख्याल रखती है";
"trashmonster_farao_description" = "फराओ थोड़ा खतरनाक है। जब उसे अपना खतरनाक कचरा नहीं मिलता है तो वह मधुमक्खी की तरह गुस्सा हो जाती है। वह जो सबसे ज्यादा पसंद करती है वह पुरानी सूखी नेल पॉलिश और हेयर स्प्रे है।";
"trashmonster_flamman_description" = "आग और गर्मी दो चीजें हैं जिनके बारे में फ्लेममैन भावुक है। वह वह सब कुछ इकट्ठा करती है जो कोई और नहीं चाहता है। डायपर, डिश ब्रश और शांत करनेवाला, कि उसका सामान है!";
"trashmonster_glas-klara_description" = "ग्लास-क्लारा ग्लास की ठीक गेंद की तरह चमकती और झिलमिलाती है कि वह है। अपनी बड़ी खूबसूरत आंखों के साथ, वह परिवेश को स्कैन करती है और बिना रंग के ग्लास की तलाश करती है, जो बेहतरीन चीजें हैं जिन्हें वह जानती है।";
"trashmonster_glasmo_description" = "ग्लास्मो को काले कांच की वस्तुओं से प्यार है लेकिन टूटे हुए कांच से क्षतिग्रस्त होने का डर है। अपने छह संकीर्ण पैरों के साथ, वह बेहतरीन बोतलों और जार को खोजने के लिए कचरे के डिब्बे और कंटेनरों के माध्यम से चुपके से घुसता है।";
"trashmonster_kompostina_description" = "केले का छिलका, सेब कोर या पास्ता, कोम्पोस्टिना भोजन के तरीकों में अधिकांश चीजों को इकट्ठा करता है। वह जानती है कि यह महत्वपूर्ण है कि हम अपने भोजन की बर्बादी को कम करें, इसलिए जब वह बचे हुए लोगों को पाती है जो वास्तव में खाद्य नहीं हैं तो वह अतिरिक्त खुश होती है।";
"trashmonster_lysien_description" = "लिसियन वास्तव में अंधेरे से डरता है, वह सोचता है कि अंधेरे में खतरनाक और डरावने राक्षस हैं। सिर के ऊपर उसका अपना एलईडी लैंप होता है और हाथ में वह हमेशा फ्लोरोसेंट लैंप अपने साथ रखता है। अंधेरा आते ही वह इन्हें रोशन करता है।";
"trashmonster_metallika_description" = "धातु सबसे अच्छा मेटलिका जानता है। वह सब कुछ इकट्ठा करता है जिसे छूना मुश्किल है और इसे हिट करने पर आवाज़ करता है, बस खुद की तरह!";
"trashmonster_nova-kras_description" = "टूटे हुए पीने के गिलास, क्रॉकरी या मिट्टी के बर्तन, ये ऐसी चीजें हैं जो नोवा-क्रास को पसंद हैं।";
"trashmonster_pantomen_description" = "पैंटोमन उन सभी डिब्बे और बोतलों को इकट्ठा करता है जिन पर जमा निशान होता है। पैंटोमन का भी उस पर एक निशान है, बस इसलिए उसे प्लास्टिस या मेटालिका के साथ मिश्रित नहीं किया जाएगा।";
"trashmonster_pappis_description" = "बहुत सारे कागज हैं, जो पप्पिस के लिए अच्छा है जो पुराने पेपर पैकेजिंग को इकट्ठा करना पसंद करते हैं। केलॉग पैकेज, शू बॉक्स और क्रीम पैकेज उनकी कुछ पसंदीदा चीजें हैं।";
"trashmonster_plastis_description" = "प्लास्टिक पैकेजिंग के लिए अच्छा है, कम से कम प्लास्टिस के अनुसार जो सोचता है कि पुरानी पैकेजिंग से आविष्कार और नए खिलौने बनाना वास्तव में मजेदार है!";
"trashmonster_prassel_description" = "प्रसेल अनंत काल तक अखबार पढ़ सकते थे। अनसुलझे क्रॉसवर्ड पहेली, मजेदार कहानियां और कल की खबरें - वह सब कुछ जो वह पढ़ना चाहती है।";
"trashmonster_press_to_learn_more" = "इसके बारे में अधिक जानने के लिए एक राक्षस टैप करें।";
"trashmonster_stubben_description" = "स्टंप पत्तियों, टहनियों और फूलों को इकट्ठा करता है और सबसे अच्छी बात यह है कि वह जो इकट्ठा करता है वह नई मिट्टी बन जाती है जिसमें आप नए पेड़ या फूल लगा सकते हैं।";
"trashmonster_textilia_description" = "टेक्स्टिलिया गिरोह के लिए बहुत नया है। वह पुराने और पहने हुए कपड़े और जूते इकट्ठा करती है, लेकिन ऐसे कपड़े भी जो अच्छे हैं और फिर से इस्तेमाल किए जा सकते हैं।";
"trashmonster_towalett_description" = "पेशाब, पोप और टॉयलेट पेपर एकमात्र चीजें हैं जो टोवेलेट एकत्र करती हैं। अपनी पांच आँखों से वह कॉटन स्वैब, तंबाकू या गीले पोंछे जैसे थिग्स की तलाश करती है, ये टॉयलेट में नहीं हैं!";
"trashmonster_wellmer_description" = "कई नाजुक पैकेज नालीदार कार्डबोर्ड में आते हैं, यही कारण है कि वेल्मर एक शूरवीर की तरह महसूस करता है जो नाजुक चीजों की रक्षा करता है। वह नालीदार कार्डबोर्ड का पुन: उपयोग करना पसंद करता है और अक्सर पुराने बक्से से शूरवीरों के महल बनाता है।";
"undp_goal" = "लक्ष्य <span translate='no'>string</span>";
"undp_goal_10_description" = "इसका मतलब है कि आप दुनिया में जहां भी रहते हैं, आपके पास हर किसी के समान अधिकार और अवसर होने चाहिए।";
"undp_goal_10_title" = "लक्ष्य 10: असमानताओं को कम करना";
"undp_goal_11_description" = "इसका मतलब है कि हर कोई सुरक्षित और अच्छे घरों में रहने में सक्षम होना चाहिए।";
"undp_goal_11_title" = "लक्ष्य 11: टिकाऊ शहर और समुदाय";
"undp_goal_12_description" = "इसका मतलब यह है कि हमें बहुत सारी चीजों को बर्बाद या फेंकने के बिना केवल उन चीजों को खरीदना चाहिए जिनकी हमें आवश्यकता है।";
"undp_goal_12_title" = "लक्ष्य 12: जिम्मेदार खपत और उत्पादन";
"undp_goal_13_description" = "हमारे ग्रह पर अच्छी तरह से रहने के लिए, हमें यह सुनिश्चित करना होगा कि जलवायु परिवर्तन को रोका जाए।";
"undp_goal_13_title" = "लक्ष्य 13: जलवायु कार्रवाई";
"undp_goal_14_description" = "इसका मतलब है कि हमें बहुत अधिक मछली को गंदगी या पकड़ने के बिना अपने महासागरों की देखभाल करनी चाहिए।";
"undp_goal_14_title" = "लक्ष्य 14: पानी के नीचे जीवन";
"undp_goal_15_description" = "इसका मतलब है कि हमें अपनी प्रकृति और वहां रहने वाले सभी लोगों, जानवरों और मनुष्यों के साथ-साथ पौधों और इनसेट का ख्याल रखना चाहिए।";
"undp_goal_15_title" = "लक्ष्य 15: भूमि पर जीवन";
"undp_goal_16_description" = "इसका मतलब है कि हमारे पास शांतिपूर्ण और न्यायपूर्ण समाज होना चाहिए।";
"undp_goal_16_title" = "लक्ष्य 16: शांति, न्याय और मजबूत संस्थान";
"undp_goal_17_description" = "एक बेहतर दुनिया बनाने के लिए हमें एक साथ, एक-दूसरे के साथ और अन्य देशों के साथ मिलकर काम करना चाहिए।";
"undp_goal_17_title" = "लक्ष्य 17: लक्ष्यों के लिए साझेदारी";
"undp_goal_1_description" = "गरीबी का मतलब यह हो सकता है कि आपके पास भोजन या धन की कमी है, सुरक्षित महसूस नहीं करते हैं, या स्कूल जाने की अनुमति नहीं है।";
"undp_goal_1_title" = "लक्ष्य 1: कोई गरीबी नहीं";
"undp_goal_2_description" = "इसका मतलब यह है कि हर किसी को अच्छे तरीके से उगाए गए भोजन को खाने में सक्षम होना चाहिए, और किसी को भी भूखा नहीं रहना चाहिए।";
"undp_goal_2_title" = "लक्ष्य 2: शून्य भूख";
"undp_goal_3_description" = "इसका मतलब है कि हर कोई स्वस्थ और अच्छा जीवन जीने में सक्षम होना चाहिए।";
"undp_goal_3_title" = "लक्ष्य 3: अच्छा स्वास्थ्य और कल्याण";
"undp_goal_4_description" = "इसका मतलब है कि हर किसी को स्कूल जाने में सक्षम होना चाहिए और हमारे पूरे जीवन में नई चीजें सीखने का अवसर मिलना चाहिए।";
"undp_goal_4_title" = "लक्ष्य 4: गुणवत्तापूर्ण शिक्षा";
"undp_goal_5_description" = "इसका मतलब है कि हर कोई समान रूप से मूल्यवान है, दोनों लड़कियां और लड़के।";
"undp_goal_5_title" = "लक्ष्य 5: लैंगिक समानता";
"undp_goal_6_description" = "इसका मतलब है कि हर कोई साफ पानी पीने में सक्षम होना चाहिए और शौचालय जाने में सक्षम होना चाहिए।";
"undp_goal_6_title" = "लक्ष्य 6: स्वच्छ पानी और स्वच्छता";
"undp_goal_7_description" = "इसका मतलब है कि हर किसी को बिजली और गर्मी में सक्षम होना चाहिए जो हमारे ग्रह के लिए अच्छा है।";
"undp_goal_7_title" = "लक्ष्य 7: सस्ती और स्वच्छ ऊर्जा";
"undp_goal_8_description" = "इसका मतलब है कि हर किसी को काम पर अच्छा महसूस करने में सक्षम होना चाहिए। आपको पूरे दिन काम नहीं करना चाहिए, बल्कि अपने खाली समय में आराम करने में भी सक्षम होना चाहिए।";
"undp_goal_8_title" = "लक्ष्य 8: सभ्य काम और आर्थिक विकास";
"undp_goal_9_description" = "इसका मतलब यह है कि हमारे आविष्कारों का निर्माण हमारे और हमारी प्रकृति के लिए स्वस्थ और अच्छा होना चाहिए।";
"undp_goal_9_title" = "लक्ष्य 9: उद्योग, नवाचार और बुनियादी ढांचा";
