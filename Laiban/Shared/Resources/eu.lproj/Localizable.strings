"trashmonster_elli_description" = "Gauza elektrikoak dira Ellisen faboritoak. Ile-lehorgailuak, tabletak eta telefono zelularrak: bera arduratzen da guzti horietaz";
"trashmonster_farao_description" = "Farao arriskutsu samarra da. Erle bat bezala haserretzen da bere hondakin arriskutsuak aurkitzen ez dituenean. Gehien gustatzen zaiona azkazal lehorreko esmaltea eta spray-a ilearentzat da.";
"trashmonster_flamman_description" = "Sua eta beroa Flamman maite duten bi gauza dira. Beste inork nahi ez duen guztia biltzen du. Pañalak, plateretarako eskuilak eta txupeteak, hori da berea!";
"trashmonster_glas-klara_description" = "Glas-Klarak distira eta distira egiten du, beirazko bola finak bezala. Bere begi handi eta ederrekin, inguruak eskaneatzen ditu eta kolorerik gabeko beira bilatzen du, dakien gauzarik onenak baitira.";
"trashmonster_glasmo_description" = "Glasmok beira iluneko gauzak maite ditu, baina beira hautsiek kalte egingo dioten beldurtuta. Sei hanka estuekin, zabor-potoen eta edukiontzien bidez irentsiko da, botila eta flaskorik onenak aurkitzeko.";
"trashmonster_kompostina_description" = "Banana-kaskara, sagar-guneak edo pasta, Kompostinak gauza gehienak elikagai moduan biltzen ditu. Berak badaki garrantzitsua dela gure elikagaiak gutxitzea, eta, beraz, oso pozik soberan aurkitzen dituenean, benetan jangarriak ez direnak.";
"trashmonster_lysien_description" = "Lysien benetan izututa iluntasunaz, iluntasunean munstro arriskutsu eta beldurgarriak daudela uste du. Bere buruaren gainean leD lanpara dauka eta bere eskuan lanpara fluoreszentea eramaten du beti. Iluntasuna erortzen hasi bezain laster pizten ditu.";
"trashmonster_metallika_description" = "Metala da Metallikak ezagutzen duen onena. Jotzen zaila den guztia biltzen du, eta soinuak egiten ditu jotzen duenean, berak bezala!";
"trashmonster_nova-kras_description" = "Edalontzi hautsiak, bajajaiak edo buztinezko eltzeak, hauek Nova-Krasi gustatzen zaizkion gauzak dira.";
"trashmonster_pantomen_description" = "Pantomenek biltegi-markak dituzten lata eta botila guztiak biltzen ditu. Pantomenek ere marka bat du bertan, Plastis edo Metallikarekin nahastu ez dadin.";
"trashmonster_pappis_description" = "Paper handia, eta hori ona da Pappisentzat, paper zaharreko ontziak biltzea gustatzen zaionarentzat. Kellog paketeak, zapata-kaxak eta krema-paketeak dira gogokoenak.";
"trashmonster_plastis_description" = "Plastikozko ontziak onak dira, Plastisen arabera behintzat, antzinako ontzietatik asmakizun eta jostailu berriak sortzea benetan dibertigarria dela uste izateko.";
"trashmonster_prassel_description" = "Prasselek egunkariak irakur zitzakeen betikotasun baten truke. Argitu gabeko gurutzegramak, istorio dibertigarriak eta atzoko albisteak: irakurri nahi duen guztia.";
"trashmonster_press_to_learn_more" = "Munstro bat jotzen du horri buruzko informazio gehiago lortzeko.";
"trashmonster_stubben_description" = "Ukitzeak hostoak, adarrak eta loreak biltzen ditu, eta onena da jasotzen duena lur berri bihurtzen dela, non zuhaitz edo lore berriak landa daitezkeen.";
"trashmonster_textilia_description" = "Textilia nahiko berria da taldearentzat. Arropa eta zapata zaharrak eta erabiliak biltzen ditu, baina baita gozoak diren jantziak ere, eta berriro erabil daitezke.";
"trashmonster_towalett_description" = "Pisa, kaka eta paper higienikoa dira Towalettek jasotzen dituen gauza bakarrak. Bere bost begiekin thig-ak bilatzen ditu, kotoizko, tabakozko isipuak edo toallita hezeak bezala, hauek ez dira kotoizkoak!";
"trashmonster_wellmer_description" = "Pakete hauskor asko kartoi kokorkatuan datoz, eta Wellmer gauza hauskorrak babesten dituen zaldun bat bezala sentitzen da. Izugarri gustatzen zaio kartoi kortua berrerabiltzea, eta sarritan zaldunen gazteluak egiten ditu kaxa zaharrekin.";
"undp_goal" = "Helburuen %@";
"undp_goal_10_description" = "Horrek esan nahi du munduan bizitzen zaran tokian, beste guztien antzeko eskubide eta aukerak izan behar dituzula.";
"undp_goal_10_title" = "10. helburua: Desberdintasunak murriztea";
"undp_goal_11_description" = "Horrek esan nahi du guztiek etxe seguru eta onetan bizi ahal izango luketela.";
"undp_goal_11_title" = "11. helburua: Hiri eta komunitate jasangarriak";
"undp_goal_12_description" = "Horrek esan nahi du behar ditugun gauzak erosi behar ditugula, alferrik galdu gabe edo gauza gehiegi bota gabe.";
"undp_goal_12_title" = "12. helburua: Kontsumoa eta ekoizpen arduratsuak";
"undp_goal_13_description" = "Gure planetan ondo bizitzeko, klima aldaketa geldiarazten dela ziurtatu behar dugu.";
"undp_goal_13_title" = "13. helburua: Ekintza klimatikoa";
"undp_goal_14_description" = "Horrek esan nahi du gure ozeanoak zaindu behar ditugula zaborrik bota gabe edo arrain gehiegi harrapatu gabe.";
"undp_goal_14_title" = "14. helburua: Itsaspeko bizitza";
"undp_goal_15_description" = "Horrek esan nahi du gure izaera eta bertan bizi diren guztiak zaindu behar ditugula, bai animaliak, bai gizakiak, bai landareak eta insetcs-ak.";
"undp_goal_15_title" = "15. helburua: Lurreko bizitza";
"undp_goal_16_description" = "Horrek esan nahi du gizarte baketsu eta zuzenak izan behar ditugula.";
"undp_goal_16_title" = "16. helburua: Bakea, justizia eta erakunde sendoak";
"undp_goal_17_description" = "Mundu hobe bat sortzeko elkarrekin lan egin behar dugu, gure artean eta beste herrialde batzuekin.";
"undp_goal_17_title" = "17. helburua: Helburuak lortzeko aliantzak";
"undp_goal_1_description" = "Pobreziak esan nahi du janaria edo dirua falta zaiola, ez dela seguru sentitzen edo ez zaiola eskolara joaten uzten.";
"undp_goal_1_title" = "1. helburua: Pobreziari ez";
"undp_goal_2_description" = "Horrek esan nahi du guztiek jan beharko luketela janaria ondo landuta, eta inork ez lukeela goserik pasa behar.";
"undp_goal_2_title" = "2. helburua: Zero gosea";
"undp_goal_3_description" = "Horrek esan nahi du guztiek bizitza osasuntsua eta ona bizi ahal izango luketela.";
"undp_goal_3_title" = "3. helburua: Osasun ona eta ongizatea";
"undp_goal_4_description" = "Horrek esan nahi du guztiek eskolara joan beharko luketela eta gure bizitzan zehar gauza berriak ikasteko aukera izan beharko luketela.";
"undp_goal_4_title" = "4. helburua: Kalitatezko hezkuntza";
"undp_goal_5_description" = "Horrek esan nahi du denak ere baliotsuak direla, bai neskatoak, bai haurrak.";
"undp_goal_5_title" = "5. helburua: Generoen arteko berdintasuna";
"undp_goal_6_description" = "Horrek esan nahi du guztiek ur garbia edan eta bainura joan beharko luketela.";
"undp_goal_6_title" = "6. helburua: Edateko ura eta saneamendua";
"undp_goal_7_description" = "Horrek esan nahi du guztiek izan beharko luketela gure planetarentzat ona den elektrizitatea eta beroa.";
"undp_goal_7_title" = "7. helburua: Energia eskuragarria eta garbia";
"undp_goal_8_description" = "Horrek esan nahi du guztiek ondo sentitu beharko luketela lanean. Egun osoan ere ez luke lan egin beharrik izan behar, aisialdian erlaxatu ahal izatea baizik.";
"undp_goal_8_title" = "8. helburua: Lan duina eta hazkunde ekonomikoa";
"undp_goal_9_description" = "Horrek esan nahi du gure asmakizunen fabrikazioa osasungarria eta ona izan behar dela guretzat eta gure izaerarentzat.";
"undp_goal_9_title" = "9. helburua: Industria, berrikuntza eta azpiegitura";
