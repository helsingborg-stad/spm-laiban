"undp_goal_12_description" = "Det betyder att vi bara ska handla saker vi behöver utan att slösa eller slänga för många saker.";
"undp_goal_12_title" = "Mål 12: Hållbar konsumtion och produktion";
"undp_goal_13_description" = "För att kunna leva bra på vår planet måste vi se till att klimatförändringarna stoppas.";
"undp_goal_13_title" = "Mål 13: Bekämpa klimatförändringarna";
"undp_goal_14_description" = "Detta betyder att vi ska ta hand om våra hav utan att skräpa ner eller ta upp för mycket fisk.";
"undp_goal_14_title" = "Mål 14: Hav och marina resurser";
"undp_goal_15_description" = "Det här betyder att vi ska ta hand om vår natur och alla som bor där, både djur och människor men även växter och insekter.";
"undp_goal_15_title" = "Mål 15: Ekosystem och biologisk mångfald";
"undp_goal_16_description" = "Det betyder att vi ska ha fredliga och rättvisa samhällen.";
"undp_goal_16_title" = "Mål 16: Fredliga och inkluderande samhällen";
"undp_goal_17_description" = "Det betyder att vi ska arbeta tillsammans och med andra länder för att skapa en bättre värld.";
"undp_goal_17_title" = "Mål 17: Genomförande och globalt partnerskap";
"undp_goal_1_description" = "Med fattigdom menas att man kanske saknar mat, pengar, inte känner sig trygg eller inte får gå i skolan.";
"undp_goal_1_title" = "Mål 1: Ingen fattigdom";
"undp_goal_2_description" = "Det betyder att alla ska kunna äta hälsosam mat som är odlad på ett bra sätt och att ingen ska behöva gå hungrig.";
"undp_goal_2_title" = "Mål 2: Ingen hunger";
"undp_goal_3_description" = "Det betyder att alla ska kunna leva ett hälsosamt och bra liv.";
"undp_goal_3_title" = "Mål 3: God hälsa och välbefinnande";
"undp_goal_4_description" = "Det betyder att alla ska kunna gå i skolan och få möjligheten att lära oss nya saker under hela våra liv.";
"undp_goal_4_title" = "Mål 4: God utbildning";
"undp_goal_5_description" = "Det betyder att alla är lika mycket värda, både flickor och pojkar.";
"undp_goal_5_title" = "Mål 5: Jämställdhet";
"undp_goal_6_description" = "Det betyder att alla ska kunna dricka rent vatten och kunna gå på toaletten.";
"undp_goal_6_title" = "Mål 6: Rent vatten och sanitet för alla";
"undp_goal_7_description" = "Det betyder att alla ska kunna ha el och värme som är bra för vår planet.";
"undp_goal_7_title" = "Mål 7: Hållbar energi för alla";
"undp_goal_8_description" = "Det betyder att alla ska kunna må bra på sitt arbete. Man ska heller inte behöva jobba hela dagen, utan även kunna koppla av på sin fritid.";
"undp_goal_8_title" = "Mål 8: Anständiga villkor och ekonomisk tillväxt";
"undp_goal_9_description" = "Detta betyder att tillverkning av våra uppfinningar ska vara hälsosamma och bra för oss och vår natur.";
"undp_goal_9_title" = "Mål 9: Hållbar industri, innovationer och infrastruktur";
"undp_press_to_learn_more" = "Tryck på ett mål för att lära dig mer om det.";
