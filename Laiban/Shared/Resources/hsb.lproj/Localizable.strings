"trashmonster_batman_description" = "Batman je nabity typ. Jako jeho energija k nachilenju dźe, poda so wón na wotkrywansku jězbu a pyta za nowymi baterijemi.";
"trashmonster_elli_description" = "Elektriske wěcy su ellisowi faworitojo. Haartrockner, tablety a handyje - wona so wo nju wšitkich stara";
"trashmonster_farao_description" = "Farao je tróšku strašny. Wona so rozhněwa kaž pčołka, hdyž jeju strašny spad njenamaka. Štož wona najbóle lubuje, je stary sušeny nochćowy lak a włosowy spray.";
"trashmonster_flamman_description" = "Woheń a horcota stej dwě wěcy, za kotrejž so flamman rozpaliće zajimuje. Wona zběra wšitko, štož hewak nichtó nochce. Pjelchi, wopłokować a čucharjo, to je jeje kutło!";
"trashmonster_glas-klara_description" = "Škleńca-jasna so frinkoli a frinkoli so kaž drobna škleńčana kula, kiž je. Ze swojim wulkim, rjanym wóčkom wona wokolinu wotprasnje a pyta za njebarbjenej škleńcu, to su najrjeńše wěcy, kiž wona znaje.";
"trashmonster_glasmo_description" = "Škleńčana mozowca lubuje ćmowe objekty, ma pak strach, so přez škleńčane črjopy wobškodźić. Ze swojimi šěsć wuskimi nohami kradnje so wón přez wotpadkowe bowy a kónki, zo by drobne bleše a škleńcy namakał.";
"trashmonster_kompostina_description" = "Bananowe šale, jabłukowe jadra abo nudle, kompostina hromadźi najwjace wěcow na wašnje. Wona wě, zo je wažne, zo swoje brojenje žiwidłow redukujemy, potajkim je wona wosebje zbožowna, hdyž zbytki namaka, kotrež njejsu woprawdźe essomne.";
"trashmonster_lysien_description" = "Lyzija ma woprawdźe strach před ćmu, wón sej mysli, zo su póćmje strašne a bojazliwe monstery. Na hłowje ma swójsku LED-lampu a w ruce nosy stajnje swěćatu lampu při kole. Wón zaswěći je, čejež započina so ćma padać.";
"trashmonster_metallika_description" = "Metal je to najlěpše, štož metallika znaje. Wón zhromadźi wšitko, štož je ćežko dótknyć a čini zwuki, hdyž so zetka, runje tak kaž wón sam!";
"trashmonster_nova-kras_description" = "Rozbite pitne škleńcy, sudobjo abo tontöpfy, to su wěcy, kiž ma nova-kras rady.";
"trashmonster_pantomen_description" = "Pantomy hromadźi wšě tyzki a bleše, na kotrychž je zastawne znamjo připrawjene. Pantomen ma tež markěrowanje na njej, jenož zo njeby so z plastisu abo metalliku zaměniła.";
"trashmonster_pappis_description" = "Je wjele papjery, štož je derje za papjercowe pisty, kotrež rady stare zapakowanja papjery hromadźa. Kellog-pakety, črijowe kartony a kremowe wobalki su někotre z jeho najlubšich wěcow.";
"trashmonster_plastis_description" = "Plastowe zapakowanja maja so derje měć, znajmjeńša wótře Plastis, kiž sej mysli, zo zestajenje wunamakankow a nowych hrajkow ze starych zapakowanjow woprawdźe wjeselo čini!";
"trashmonster_prassel_description" = "Prassel móžeše wěčnosć dołho nowiny čitać. Njerozrisane křižowki, lóštne stawiznički a powěsće wo wčerawšim - WŠITKO, štož chce čitać.";
"trashmonster_press_to_learn_more" = "Pokiw na monster, zo by wjace wo tym zhonił.";
"trashmonster_stubben_description" = "Tup hromadźi łopjena, hałužki a kćenja a to najlěpše wot wšeho, zo budźe to, štož chce hromadźić, k nowej zemi, w kotrejž móžeće nowe štomy abo kwětki sadźeć.";
"trashmonster_textilia_description" = "tekstilija je chětro nowa w bandy. Wona zběra staru a dodźeržanu drastu a črije, ale tež drasty, kiž su rjane a zaso wužiwać hodźa.";
"trashmonster_towalett_description" = "Piss, kakawa a nuznikowa papjera su jeničke wěcy, kiž towalet hromadźi. Ze swojimi pjeć wočemi pyta za tigami kaž watowymi štabikami, tobakom abo włóžnymi rubiškami, tute njesłušeja do nuznika!";
"trashmonster_wellmer_description" = "Wjele rozdrjebjenych zapakowanjow přińdźe we wellpappe, čehoždla so wellmer kaž ryćer čuje, kiž rozłamliwe wěcy škita. Wón to lubuje, wellpappe zaso wužiwać a čini husto ryćerske hrody ze starych kistow.";
"undp_goal" = "Zaměr %@";
"undp_goal_10_description" = "To woznamjenja, zo Wy, hdźežkuli tež Wy na swěće bydla, podobne prawa a móžnosće měć kaž wšitcy druzy.";
"undp_goal_10_title" = "Zaměr 10: Mjenje njerunosćow";
"undp_goal_11_description" = "To woznamjenja, zo měł kóždy kmany być, we wěstych a dobrych domach žiwy być.";
"undp_goal_11_title" = "Zaměr 11: Naslědne města a gmejny";
"undp_goal_12_description" = "To woznamjenja, zo měli jenož wěcy kupić, kotrež trjebamy, bjez toho zo přewjele wěcow brojimy abo preč mjetamy.";
"undp_goal_12_title" = "Zaměr 12: zamołwity konsum a produkcija";
"undp_goal_13_description" = "Zo bychmy na našim planeće derje žiwi być móhli, dyrbimy so wo to starać, zo so klimowa změna zadźerži.";
"undp_goal_13_title" = "Zaměr 13: škit klimy";
"undp_goal_14_description" = "To woznamjenja, zo měli so wo naše oceany starać, bjez toho zo bychmy přewjele ryby wotpadkowali abo łójili.";
"undp_goal_14_title" = "Zaměr 14: žiwjenje pod wodu";
"undp_goal_15_description" = "To woznamjenja, zo měli so wo našu přirodu a wšěch, kotřiž tam bydla, starać, zwěrjata kaž tež ludźi kaž tež rostliny a insetcs.";
"undp_goal_15_title" = "Ziel 15: žiwjenje na kraju";
"undp_goal_16_description" = "To woznamjenja, zo měli měrliwe a sprawne towaršnosće měć.";
"undp_goal_16_title" = "Zaměr 16: měr, sprawnosć a sylne institucije";
"undp_goal_17_description" = "Zo bychmy lěpši swět wutworili, měli hromadźe dźěłać, mjez sobu a z druhimi krajemi.";
"undp_goal_17_title" = "Zaměr 17: Partnerstwa za zaměry";
"undp_goal_1_description" = "Chudoba móže rěkać, zo ći to na cyrobje abo pjenjezy pobrachuje, ty so wěsty nječuješ abo njesměš do šule chodźić.";
"undp_goal_1_title" = "Ziel 1: Žana chudoba";
"undp_goal_2_description" = "To woznamjenja, zo měł kóždy kmany być, žiwidła jěsć, kotrež so na dobre wašnje plahuja, a zo njetrjeba nichtó hłód tradać.";
"undp_goal_2_title" = "Ziel 2: nula hłód";
"undp_goal_3_description" = "To woznamjenja, zo měł kóždy kmany być, strowe a dobre žiwjenje wjesć.";
"undp_goal_3_title" = "Cil 3: strowota a derjeměće";
"undp_goal_4_description" = "To woznamjenja, zo měł kóždy kmany być, do šule chodźić a móžnosć měć, w běhu našeho žiwjenja nowe wěcy wuknyć.";
"undp_goal_4_title" = "Zaměr 4: Wysokohódnotne kubłanje";
"undp_goal_5_description" = "To woznamjenja, zo su wšitcy jenak drohotni, holcy kaž tež hólcy.";
"undp_goal_5_title" = "Zaměr 5: runostajenje splahow";
"undp_goal_6_description" = "To woznamjenja, zo měł kóždy kmany być, čistu wodu pić a na nuzz trěbny kaznju hić móc.";
"undp_goal_6_title" = "Zaměr 6: Čista woda a sanitarne zarjadnišća";
"undp_goal_7_description" = "To woznamjenja, zo měł kóždy kmany być, měć milinu a ćopłotu, kotrejž stej derje za naš planet.";
"undp_goal_7_title" = "Zaměr 7: Zapłaćomna a čista energija";
"undp_goal_8_description" = "To woznamjenja, zo měł so kóždy při dźěle derje čuć móc. Wy njeměli tež cyły dźeń dźěłać dyrbjeć, ale tež we Wašim swobodnym času wotpinać móc.";
"undp_goal_8_title" = "Zaměr 8: Čłowjeske dźěło a rozrost hospodarstwo";
"undp_goal_9_description" = "To woznamjenja, zo dyrbi zhotowjenje našich wunamakankow strowe a dobre za nas a našu přirodu być.";
"undp_goal_9_title" = "Zaměr 9: industrija, inowacija a infrastruktura";
