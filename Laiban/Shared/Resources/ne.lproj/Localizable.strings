"trashmonster_aqua-liv_description" = "एक्वा-लिभ समयको बिहानदेखि अस्तित्वमा छ, र डायनासोर र वाइकिंग्सलाई पनि भेटेको छ! एक्वा-लिभले कुनै फोहोर संकलन गर्दैन तर संसारको सबै पानीलाई नियन्त्रण गर्दछ।";
"trashmonster_batman_description" = "बैटमैन एक चार्ज वाला लड़का है। जब उसको ऊर्जा समाप्त हुन थाल्छ, ऊ खोजको यात्रामा जान्छ र नयाँ ब्याट्रीहरू खोज्छ।";
"trashmonster_elli_description" = "इलेक्ट्रिकल चीजहरू एलिसको मनपर्ने चीजहरू हुन्। हेयर ड्रायर, ट्याब्लेट र सेल फोन - उनी ती सबैको ख्याल राख्छिन्";
"trashmonster_farao_description" = "फराओ थोड़ा खतरनाक है। आफ्नो खतरनाक फोहर नभेटेपछि उनी माहुरीजस्तै रिसाउँछिन् । उनलाई सबैभन्दा बढी मन पर्ने कुरा भनेको पुरानो सुकेको नेल पालिस र हेयर स्प्रे हो।";
"trashmonster_flamman_description" = "आगो र ताप दुई चीजहरू हुन् जुन फ्लेमम्यानको बारेमा भावुक छ। उसले अरू कसैले नचाहेको सबै कुरा जम्मा गर्छे। डायपर, डिश ब्रश र प्यासिफायरहरू, त्यो उनको सामान हो!";
"trashmonster_glas-klara_description" = "ग्लास-क्लारा ग्लासको राम्रो बल जस्तै चम्किरहेको छ र झिम्काउँदछ जुन उनी हुन्। आफ्नो ठूलो सुन्दर आँखाको साथ, उनी वरपरको स्क्यान गर्छिन् र रङ्गहीन ग्लासको खोजी गर्छिन्, जुन उनलाई थाहा भएको सबै भन्दा राम्रो चीजहरू हुन्।";
"trashmonster_glasmo_description" = "ग्लास्मोलाई गाढा सिसाका वस्तुहरू मन पर्छ तर भाँचिएको सिसाले क्षति पुऱ्याउन डराउँछ। आफ्नो छ वटा साँघुरो खुट्टाले ऊ फोहोरको डिब्बा र कन्टेनरहरू छिचोल्दै उत्कृष्ट बोतल हरू र जारहरू फेला पार्छ।";
"trashmonster_kompostina_description" = "केराको बोक्रा, स्याउको कोर वा पास्ता, कोम्पोस्टिनाले खानाको तरिकामा अधिकांश चीजहरू सङ्कलन गर्दछ। तिनलाई थाह छ कि हामीले हाम्रो खानाको बर्बादी कम गर्नु महत्त्वपूर्ण छ, त्यसैले जब उनी वास्तवमा खान योग्य नभएको बचेका चीजहरू भेट्टाउँदछिन् तब उनी थप खुसी हुन्छिन्।";
"trashmonster_lysien_description" = "लिसियन साँच्चै अँध्यारोदेखि डराउँदछ, उसले सोच्दछ कि अँध्यारोमा खतरनाक र डरलाग्दो राक्षसहरू छन्। उसको टाउकोमाथि उसको आफ्नै एलईडी बत्ती छ र उसको हातमा ऊ सँधै फ्लोरोसेंट बत्ती बोक्छ। अँध्यारो ढल्न थाल्नेबित्तिकै उहाँले यी बत्तीहरू बाल्नुहुन्छ।";
"trashmonster_metallika_description" = "मेटल मेटालिकालाई सबैभन्दा राम्रो थाहा छ। उहाँले छुन गाह्रो छ कि सबै सङ्कलन र यो हिट गर्दा आवाज बनाउँछ, बस आफैलाई जस्तै!";
"trashmonster_nova-kras_description" = "भाँचिएको पिउने चश्मा, क्रॉकरी वा माटोको भाँडाहरू, यी चीजहरू हुन् जुन नोभा-क्रासमन पराउँछन्।";
"trashmonster_pantomen_description" = "प्यान्टोमेनले सबै क्यान हरू र बोतलहरू सङ्कलन गर्दछ जुन तिनीहरूमा डिपोजिट चिन्ह छ। प्यान्टोमेनमा पनि उनको छाप छ, त्यसैले उनी प्लास्टिस वा मेटालिकासँग मिसाइने छैनन्।";
"trashmonster_pappis_description" = "त्यहाँ प्रशस्त कागज छ, जुन पप्पिसको लागि राम्रो छ जसले पुरानो कागज प्याकेजिंग सङ्कलन गर्न मन पराउँछ। केलग प्याकेजहरू, जूता बक्सहरू र क्रीम प्याकेजहरू उनको मनपर्ने चीजहरू हुन्।";
"trashmonster_plastis_description" = "प्लास्टिक प्याकेजिंग राम्रो छ, कम से कम प्लास्टिसको अनुसार जसले सोच्दछ कि पुरानो प्याकेजिंगबाट आविष्कार र नयाँ खिलौनाहरू सिर्जना गर्नु साँच्चै रमाइलो छ!";
"trashmonster_prassel_description" = "प्रसेल अनन्तकालसम्म अखबार पढ्न सक्थे। अनसुलझे क्रसवर्ड पजलहरू, रमाइला कथाहरू र हिजोका समाचारहरू - सबै कुरा उनी पढ्न चाहन्छन्।";
"trashmonster_press_to_learn_more" = "यसको बारेमा थप जान्नको लागि राक्षसलाई ट्याप गर्नुहोस्।";
"trashmonster_stubben_description" = "स्टम्पले पातहरू, हाँगाहरू र फूलहरू सङ्कलन गर्दछ र सबै भन्दा राम्रो यो हो कि उसले संकलन गरेको नयाँ माटो बन्छ जसमा तपाईं नयाँ रूखहरू वा फूलहरू रोप्न सक्नुहुन्छ।";
"trashmonster_textilia_description" = "टेक्स्टिलिया गिरोहको लागि एकदम नयाँ छ। उनी पुराना र जीर्ण लुगा हरू र जुत्ताहरू जम्मा गर्छिन्, तर राम्रो र फेरि प्रयोग गर्न सकिने लुगाहरू पनि।";
"trashmonster_towalett_description" = "पिसाब, पोप र ट्वाइलेट पेपर मात्र टोवालेटले सङ्कलन गर्ने चीजहरू हुन्। आफ्नो पाँच वटा आँखाले उनी कपासको डल्लो, सुर्ती वा भिजेको वाइप्स जस्ता चिउराहरू खोज्छिन्, यी शौचालयमा हुँदैनन्!";
"trashmonster_wellmer_description" = "धेरै नाजुक प्याकेजहरू नालीदार कार्डबोर्डमा आउँछन्, यही कारण हो कि वेलमर एक नाइट जस्तै महसुस गर्दछ जसले नाजुक चीजहरूको रक्षा गर्दछ। उनी नालीदार कार्डबोर्ड पुन: प्रयोग गर्न मन पराउँछन् र प्रायः पुरानो बाकसबाट नाइट्सको महल बनाउँदछन्।";
"undp_goal" = "लक्ष्य <span translate='no'>string</span>";
"undp_goal_10_description" = "यसको अर्थ यो हो कि तपाईं संसारमा जहाँ सुकै बस्नुहुन्छ, तपाईंसँग अरू जस्तै समान अधिकार र अवसरहरू हुनुपर्दछ।";
"undp_goal_10_title" = "लक्ष्य १०: असमानता घटाउने";
"undp_goal_11_description" = "यसको मतलब सबैले सुरक्षित र राम्रो घरमा बस्न पाउनुपर्छ।";
"undp_goal_11_title" = "लक्ष्य 11: दिगो शहरहरू र कम्यूनिटीहरू";
"undp_goal_12_description" = "यसको अर्थ हामीले धेरै चीजहरू खेर नफालेर वा फ्याँकेर मात्र हामीलाई आवश्यक पर्ने वस्तुहरू किन्नुपर्छ।";
"undp_goal_12_title" = "लक्ष्य १२: जिम्मेवार उपभोग र उत्पादन";
"undp_goal_13_description" = "हाम्रो ग्रहमा राम्रोसँग बाँच्नको लागि, हामीले यो सुनिश्चित गर्नु पर्दछ कि जलवायु परिवर्तन रोकियोस्।";
"undp_goal_13_title" = "लक्ष्य 13: जलवायु कार्रवाई";
"undp_goal_14_description" = "यसको अर्थ हामीले धेरै माछा नफ्याँक्ने वा नसमात्ने गरी समुद्रको हेरचाह गर्नुपर्छ।";
"undp_goal_14_title" = "लक्ष्य १४: पानीमुनिको जीवन";
"undp_goal_15_description" = "यसको अर्थ हामीले हाम्रो प्रकृति र त्यहाँ बस्ने सबैको हेरचाह गर्नुपर्छ, जनावर र मानिस दुवैका साथै बोटबिरुवा र इन्सेटहरू।";
"undp_goal_15_title" = "लक्ष्य १५: भूमिमा जीवन";
"undp_goal_16_description" = "यसको अर्थ हामीले शान्तिपूर्ण र न्यायपूर्ण समाज पाउनुपर्छ भन्ने हो ।";
"undp_goal_16_title" = "लक्ष्य १६: शान्ति, न्याय र सुदृढ संस्थाहरू";
"undp_goal_17_description" = "राम्रो संसार निर्माण गर्न हामीले एकअर्कासँग र अन्य देशहरूसँग मिलेर काम गर्नुपर्छ।";
"undp_goal_17_title" = "लक्ष्य १७: गोलका लागि साझेदारी";
"undp_goal_1_description" = "गरिबीको अर्थ तपाईंसँग खाना वा पैसाको अभाव हुन सक्छ, सुरक्षित महसुस गर्नुहुन्न वा स्कूल जान दिनुहुन्न।";
"undp_goal_1_title" = "लक्ष्य 1: गरीबी नहीं";
"undp_goal_2_description" = "यसको अर्थ सबैले राम्रो तरिकाले फलेको खाना खान पाउनुपर्छ र कोही पनि भोकै बस्नु पर्दैन भन्ने हो।";
"undp_goal_2_title" = "लक्ष्य 2: शून्य भूख";
"undp_goal_3_description" = "यसको अर्थ सबैले स्वस्थ र राम्रो जीवन जिउन पाउनुपर्छ ।";
"undp_goal_3_title" = "लक्ष्य ३: राम्रो स्वास्थ्य र कल्याण";
"undp_goal_4_description" = "यसको अर्थ सबैले स्कूल जान पाउनुपर्छ र जीवनभर नयाँ कुरा सिक्ने अवसर पाउनुपर्छ ।";
"undp_goal_4_title" = "लक्ष्य ४: गुणस्तरीय शिक्षा";
"undp_goal_5_description" = "यसको अर्थ के हो भने केटा र केटी दुवै समान रूपमा मूल्यवान् छन् ।";
"undp_goal_5_title" = "लक्ष्य ५: लैङ्गिक समानता";
"undp_goal_6_description" = "यसको अर्थ सबैले सफा पानी पिउन पाउनुपर्छ र शौचालय जान पाउनुपर्छ ।";
"undp_goal_6_title" = "लक्ष्य ६: स्वच्छ पानी र सरसफाई";
"undp_goal_7_description" = "यसको अर्थ यो हो कि सबैले बिजुली र ताप प्राप्त गर्न सक्षम हुनुपर्दछ जुन हाम्रो ग्रहको लागि राम्रो छ।";
"undp_goal_7_title" = "लक्ष्य 7: किफायती र स्वच्छ ऊर्जा";
"undp_goal_8_description" = "यसको मतलब सबैले काममा राम्रो महसुस गर्न सक्षम हुनुपर्छ। तपाईं पनि सबै दिन काम गर्न छैन, तर पनि आफ्नो खाली समयमा आराम गर्न सक्षम हुनुपर्छ.";
"undp_goal_8_title" = "लक्ष्य ८: मर्यादित काम र आर्थिक वृद्धि";
"undp_goal_9_description" = "यसको अर्थ यो हो कि हाम्रा आविष्कारहरूको निर्माण हामी र हाम्रो प्रकृतिको लागि स्वस्थ र राम्रो हुनुपर्दछ।";
