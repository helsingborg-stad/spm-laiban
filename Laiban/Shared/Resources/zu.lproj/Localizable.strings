"undp_goal_2_description" = "Lokhu kusho ukuthi wonke umuntu kufanele akwazi ukudla ukudla okukhuliswe ngendlela enhle, nokuthi akekho umuntu okufanele alambe.";
"undp_goal_2_title" = "Umgomo 2: Indlala ye-Zero";
