"undp_goal_14_description" = "Lokhu kusho ukuthi kufanele sinakekele ulwandle lwethu ngaphandle kokulahla udoti noma ukubamba izinhlanzi eziningi kakhulu.";
"undp_goal_14_title" = "Umgomo 14: Ukuphila ngaphansi kwamanzi";
