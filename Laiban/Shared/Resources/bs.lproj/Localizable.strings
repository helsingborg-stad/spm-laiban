"trashmonster_flamman_description" = "Vatra i vrućina su dvije stvari zbog kojih je Flamman strastven. Ona skuplja sve što niko drugi ne želi. Pelene, četke za sudove i cicifere, to su njene stvari!";
"trashmonster_glas-klara_description" = "Glas-Klara svjetluca i svjetluca kao fina staklena kugla koja je ona. Svojim velikim lijepim okom skenira okolinu i traži nekolorirano staklo, koje su najbolje stvari koje zna.";
"trashmonster_glasmo_description" = "Glasmo voli predmete od tamnog stakla ali je prestravljen od oštećenja razbijenim staklom. Sa svojih šest uskih nogu, ušulja se kroz kante za smeće i kontejnere kako bi pronašao najbolje boce i tegle.";
"trashmonster_kompostina_description" = "Koru od banana, jezgre jabuke ili tjestenine, Kompostina skuplja većinu stvari na načine hrane. Ona zna da je važno da smanjimo naš otpad od hrane, tako da je ekstra sretna kada pronađe ostatke koji zapravo nisu jestivi.";
"trashmonster_lysien_description" = "Lysien se stvarno boji mraka, misli da u mraku postoje opasna i strašna čudovišta. Na vrhu glave ima svoju LED lampu i u ruci uvijek nosi fluorescentnu lampu sa sobom. Zapali ih čim tama počne padati.";
"trashmonster_metallika_description" = "Metal je najbolje što Metallika zna. Skuplja sve što je teško dotaknuti i pravi zvukove kad ga udari, baš kao i on sam!";
"trashmonster_nova-kras_description" = "Slomljene čaše za piće, posude od prepeče ili gline, to su stvari koje Nova-Kras voli.";
"trashmonster_pantomen_description" = "Pantomen skuplja sve limucine i boce koje imaju oznaku depozita na njima. Pantomen također ima oznaku na njoj, samo da ne bi bila pomiješana sa Plastisom ili Metalikom.";
"trashmonster_pappis_description" = "Ima dosta papira, što je dobro za Papisa koji voli skupljati staru papirnu ambalažu. Kellog paketi, kutije za cipele i krem paketi su neke od njegovih omiljenih stvari.";
"trashmonster_plastis_description" = "Plastično pakiranje je dobro imati, barem prema Plastisu koji misli da je stvaranje izuma i novih momaka iz stare ambalaže zaista zabavno!";
"trashmonster_prassel_description" = "Prassel je mogao čitati novine vječno. Neriješene križaljke, smiješne priče i jučerašnje vijesti - SVE što želi pročitati.";
"trashmonster_press_to_learn_more" = "Tapni čudovište da saznaš više o tome.";
"trashmonster_stubben_description" = "Panj skuplja lišće, grančice i cvijeće a najbolje od svega je da ono što sakuplja postaje novo tlo u kojem možete posaditi novo drveće ili cvijeće.";
"trashmonster_textilia_description" = "Textilia je prilično nova za bandu. Skuplja staru i istrošenu odjeću i obuću, ali i odjeću koja je lijepa i može se ponovno koristiti.";
"trashmonster_towalett_description" = "Piša, kaka i toaletni papir su jedine stvari koje Towalett skuplja. Sa pet očiju traži mrvice poput pamuka, duhana ili mokarih brisa, ove ne pripadaju u WC šolju!";
"trashmonster_wellmer_description" = "Mnogi krhki paketi dolaze u koraguiranom kartonu, zbog čega se Wellmer osjeća kao vitez koji štiti krhke stvari. Voli da koristi koruski karton i često pravi vitezove zamke od starih kutija.";
"undp_goal" = "Gol %@";
"undp_goal_10_description" = "To znači da gdje god živite u svijetu, trebali biste imati slična prava i prilike kao i svi ostali.";
"undp_goal_10_title" = "Cilj 10: Smanjene nejednakosti";
"undp_goal_11_description" = "To znači da bi svi trebali moći živjeti u sigurnim i dobrim domovima.";
"undp_goal_11_title" = "Cilj 11: Održivi gradovi i komunalije";
"undp_goal_12_description" = "To znači da trebamo kupovati samo stvari koje su nam trebale bez bacanja ili bacanja previše stvari.";
"undp_goal_12_title" = "Cilj 12: Odgovorna potrošnja i proizvodnja";
"undp_goal_13_description" = "Da bismo dobro živjeli na našoj planeti, moramo osigurati da klimatske promjene budu zaustavljene.";
"undp_goal_13_title" = "Cilj 13: Klimatska akcija";
"undp_goal_14_description" = "To znači da treba da se pobrinemo za naše okeane bez legla ili hvatanja previše ribe.";
"undp_goal_14_title" = "Cilj 14: Život ispod vode";
"undp_goal_15_description" = "To znači da treba da se pobrinemo za našu prirodu i sve koji tamo žive, i životinje i ljudi kao i biljke i insetke.";
"undp_goal_15_title" = "Cilj 15: Život na kopnu";
"undp_goal_16_description" = "To znači da bi trebali imati mirna i samo suština.";
"undp_goal_16_title" = "Cilj 16: Mir, pravda i jake institucije";
"undp_goal_17_description" = "Da bi stvorili bolji svijet trebali bi raditi zajedno, jedni s drugima, i sa drugim zemljama.";
"undp_goal_17_title" = "Cilj 17: Partnerstva za ciljeve";
"undp_goal_1_description" = "Siromaštvo može značiti da vam nedostaje hrane ili novca, ne osjećate se sigurno, ili vam nije dozvoljeno da idete u školu.";
"undp_goal_1_title" = "Cilj 1: Nema siromaštva";
"undp_goal_2_description" = "To znači da bi svi trebali moći jesti hranu uzgojenu na dobar način, i da nitko ne bi trebao ići gladan.";
"undp_goal_2_title" = "Cilj 2: Nula gladi";
"undp_goal_3_description" = "To znači da bi svi trebali moći živjeti zdrav i dobar život.";
"undp_goal_3_title" = "Cilj 3: Dobro zdravlje i zdravlje";
"undp_goal_4_description" = "To znači da bi svi trebali biti u mogućnosti da idu u školu i da imaju priliku da uče nove stvari tokom našeg života.";
"undp_goal_4_title" = "Cilj 4: Kvalitetno obrazovanje";
"undp_goal_5_description" = "To znači da su svi jednako vrijedni, i djevojčice i dječaci.";
"undp_goal_5_title" = "Cilj 5: Ravnopravnost spolova";
"undp_goal_6_description" = "To znači da bi svi trebali biti u mogućnosti piti čistu vodu i moći otići u WC.";
"undp_goal_6_title" = "Cilj 6: Čista voda i sanitarna";
"undp_goal_7_description" = "To znači da bi svi trebali moći imati struju i toplinu koja je dobra za našu planetu.";
"undp_goal_7_title" = "Cilj 7: Pristupačna i čista energija";
"undp_goal_8_description" = "To znači da bi se svi trebali moći osjećati dobro na poslu. Takođe ne bi trebalo da radite ceo dan, ali i da budete u mogućnosti da se opustite u slobodno vreme.";
"undp_goal_8_title" = "Cilj 8: Pristojan rad i ekonomski rast";
"undp_goal_9_description" = "To znači da proizvodnja naših izuma mora biti zdrava i dobra za nas i našu prirodu.";
"undp_goal_9_title" = "Cilj 9: Industrija, inovacije i infrastruktura";
